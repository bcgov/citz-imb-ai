  <actname>Food and Agricultural Products Classification Act</actname>
  <yearenacted>2016</yearenacted>
  <chapter>1</chapter>
  <change section="8">
    <changenote eff="March 30, 2023">Section 8 (a) BEFORE amended by <chapnum>2023-10-280</chapnum>, effective March 30, 2023 (Royal Assent).</changenote>
    <bcl:paragraph id="d0e1010">
      <bcl:num>a</bcl:num>
      <bcl:text>hold himself or herself out as being certified under the certification program unless</bcl:text>
      <bcl:subparagraph id="d0e1018">
        <bcl:num>i</bcl:num>
        <bcl:text>the person is certified in accordance with the regulations, and</bcl:text>
      </bcl:subparagraph>
      <bcl:subparagraph id="d0e1027">
        <bcl:num>ii</bcl:num>
        <bcl:text>the person's certification is not suspended;</bcl:text>
      </bcl:subparagraph>
    </bcl:paragraph>
  </change>
