  <reg:content>
    <bcl:section id="d2e48" toc="false">
      <bcl:marginalnote>Included organizations</bcl:marginalnote>
      <bcl:num>1</bcl:num>
      <bcl:text>The following organizations are included in the government reporting
        entity:</bcl:text>
      <bcl:paragraph id="d2e59">
        <bcl:num>a</bcl:num>
        <bcl:text>Canadian Blood Services;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e68">
        <bcl:num>b</bcl:num>
        <bcl:text>Repealed. [B.C. Reg. 50/2004, s. 1 (a).]</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e78">
        <bcl:num>c</bcl:num>
        <bcl:text>Legal Services Society;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e87">
        <bcl:num>d)-(e</bcl:num>
        <bcl:text>Repealed. [B.C. Reg. 82/2019, s. 1 (a).]</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e105">
        <bcl:num>f</bcl:num>
        <bcl:text>Health Employers Association of British Columbia;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e114">
        <bcl:num>g</bcl:num>
        <bcl:text>Community Social Services Employers' Association of British Columbia
          (CSSEA);</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e123">
        <bcl:num>h</bcl:num>
        <bcl:text>British Columbia Public School Employers' Association;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e133">
        <bcl:num>i</bcl:num>
        <bcl:text>The Post-Secondary Employers' Association;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e142">
        <bcl:num>j</bcl:num>
        <bcl:text>Crown Corporations Employers Association;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e151">
        <bcl:num>k</bcl:num>
        <bcl:text>Repealed. [B.C. Reg. 82/2019, s. 1 (a).]</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e160">
        <bcl:num>l</bcl:num>
        <bcl:text>British Columbia Council for International Education;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d1e162">
        <bcl:num>m</bcl:num>
        <bcl:text>Forest Enhancement Society of British Columbia.</bcl:text>
      </bcl:paragraph>
      <bcl:hnote>[am. B.C. Regs. 50/2004, s. 1 (a); 30/2008; 28/2009; 29/2011, Sch. s. 2; 23/2012, s. (a); 55/2012; 82/2019.]</bcl:hnote>
    </bcl:section>
    <bcl:section id="d2e173">
      <bcl:marginalnote>Excluded organizations</bcl:marginalnote>
      <bcl:num>2</bcl:num>
      <bcl:text>The following organizations are excluded from the government reporting
        entity:</bcl:text>
      <bcl:paragraph id="d2e184">
        <bcl:num>a</bcl:num>
        <bcl:text>C2T2 The Centre for Curriculum, Transfer and Technology Society;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e193">
        <bcl:num>b</bcl:num>
        <bcl:text>CEISS Centre for Education Information Standards and Services Society;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e203">
        <bcl:num>c</bcl:num>
        <bcl:text>Insurance Council of British Columbia;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e212">
        <bcl:num>d</bcl:num>
        <bcl:text>Workers' Compensation Board;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e221">
        <bcl:num>e) to (g</bcl:num>
        <bcl:text>Repealed. [B.C. Reg. 50/2004, s. 1 (b).]</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e230">
        <bcl:num>h</bcl:num>
        <bcl:text>Credit Union Deposit Insurance Corporation of British Columbia;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d3e240">
        <bcl:num>i</bcl:num>
        <bcl:text>British Columbia Investment Management Corporation.</bcl:text>
      </bcl:paragraph>
      <bcl:hnote>[am. B.C. Regs. 50/2004; 28/2007; 20/2020.]</bcl:hnote>
    </bcl:section>
