  <reg:content>
    <bcl:section toc="false" id="d1e31">
      <bcl:marginalnote>Definition</bcl:marginalnote>
      <bcl:num>1</bcl:num>
      <bcl:text>In this regulation, <in:term>Act</in:term> means the <bcl:link resource="leg" xlink:href="/legislation/96157_00" xlink:type="locator"><in:doc>Forest Act</in:doc></bcl:link>.</bcl:text>
    </bcl:section>
    <bcl:section id="d1e46">
      <bcl:marginalnote>Information sharing â€” prescribed enactments</bcl:marginalnote>
