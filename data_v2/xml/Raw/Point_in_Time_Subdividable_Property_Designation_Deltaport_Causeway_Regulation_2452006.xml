  <acttitle>British Columbia Railway Act</acttitle>
  <title>Subdividable Property Designation (Deltaport Causeway) Regulation</title>
  <regnum>245/2006</regnum>
  <change regulation="">
    <changenote eff="March 29, 2017">Regulation BEFORE repealed by BC Reg <amendingreg>122/2017</amendingreg>, effective March 29, 2017.</changenote>
    <reg:regulation index="">
      <reg:regnum>245/2006</reg:regnum>
      <reg:oic>O.C. 651/2006</reg:oic>
      <reg:deposited>September 8, 2006</reg:deposited>
      <reg:acttitle>British Columbia Railway Act</reg:acttitle>
      <reg:title>Subdividable Property Designation (Deltaport Causeway) Regulation</reg:title>
      <reg:content>
        <bcl:section id="d2e24" toc="false">
          <bcl:marginalnote>Designation of property</bcl:marginalnote>
          <bcl:num>1</bcl:num>
          <bcl:text>The following property is designated as subdividable property as that property includes more real property than is necessary for the provision of revitalization rail services:</bcl:text>
          <oasis:table style="2">
            <oasis:tgroup cols="2" width="700">
              <oasis:colspec colname="c1" colnum="1" align="center" border="1"/>
              <oasis:colspec colname="c2" colnum="2" align="center" border="1"/>
              <oasis:thead>
                <oasis:trow>
                  <oasis:entry colname="c1" align="center">
                    <oasis:line>
                      <in:strong>Parcel Identifier No.</in:strong>
                    </oasis:line>
                  </oasis:entry>
                  <oasis:entry colname="c2" align="center">
                    <oasis:line>
                      <in:strong>Legal Description</in:strong>
                    </oasis:line>
                  </oasis:entry>
                </oasis:trow>
              </oasis:thead>
              <oasis:tbody>
                <oasis:trow>
                  <oasis:entry colname="c1" valign="top" align="center">
                    <oasis:line>026‑630‑745</oasis:line>
                  </oasis:entry>
                  <oasis:entry colname="c2" valign="top">
                    <oasis:line>Parcel A Bed of the Strait of Georgia Group 2 New Westminster District Plan BCP22581</oasis:line>
                  </oasis:entry>
                </oasis:trow>
              </oasis:tbody>
            </oasis:tgroup>
          </oasis:table>
        </bcl:section>
        <reg:provisionsnote>[Provisions of the <in:doc>British Columbia Railway Act</in:doc>, R.S.B.C. 1996, c. 36, relevant to the enactment of this regulation: section 45 (2)]</reg:provisionsnote>
      </reg:content>
    </reg:regulation>
  </change>
