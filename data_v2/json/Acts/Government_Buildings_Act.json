                        },
                        {
                            "@id": "d2e73",
                            "bcl:marginalnote": "Regulations governing use of grounds of public buildings",
                            "bcl:num": "34",
                            "bcl:subsection": [
                                {
                                    "@id": "d2e81",
                                    "bcl:num": "1",
                                    "bcl:text": "The Lieutenant Governor in Council may make regulations, either general or particular in their application, permitting, regulating or prohibiting use by the public or by any persons of",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e90",
                                            "bcl:num": "a",
                                            "bcl:text": "any path, lane, passageway or driveway through land belonging to the government leading to or from a government building, highway or public work belonging to the government, whether or not it has been dedicated or established as a public highway, or"
                                        },
                                        {
                                            "@id": "d2e99",
                                            "bcl:num": "b",
                                            "bcl:text": "the gardens, grounds or areas belonging to or used in connection with a government building, highway or public work belonging to the government."
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e109",
                                    "bcl:num": "2",
                                    "bcl:text": "The Lieutenant Governor in Council may impose penalties for enforcing regulations made under this section."
                                },
                                {
                                    "@id": "d2e118",
                                    "bcl:num": "2.1",
                                    "bcl:text": {
