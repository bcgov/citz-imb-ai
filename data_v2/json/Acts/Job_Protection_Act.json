        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e21",
                    "bcl:marginalnote": "Repealed",
                    "bcl:num": "1-19",
                    "bcl:text": "[Repealed RS1996-240-20 (1) and B.C. Reg. 85/2002.]"
                },
                {
                    "@id": "d2e33",
                    "bcl:marginalnote": "Sunset provision",
                    "bcl:num": "20",
                    "bcl:subsection": [
                        {
                            "@id": "d2e41",
                            "bcl:num": "1",
                            "bcl:text": "Sections 1 to 19 may be repealed on or after April 12, 1999 by regulation of the\n          Lieutenant Governor in Council."
                        },
                        {
                            "@id": "d2e50",
                            "bcl:num": "2",
                            "bcl:text": "The repeal authorized by subsection\u00a0(1) does not affect an economic plan or\n          agreement that has force and effect under section\u00a09 immediately before the date of the\n          repeal, and sections\u00a01 to\u00a019 are deemed to remain in force on and after that date to the\n          extent necessary to give the economic plan and the agreement force and effect according to\n          their tenor."
                        }
                    ]
                }
            ],
            "bcl:schedule": {
                "bcl:scheduletitle": "Schedule",
                "bcl:centertext": "[Schedule repealed 2002-48-16.]"
            }
        }
    }
