        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e18",
                    "bcl:marginalnote": "Definition",
                    "bcl:num": "1",
                    "bcl:text": {
                        "in:term": "Agreement",
                        "#text": "In this Act,  means the New West Partnership Trade\n        Agreement entered into by the governments of British Columbia, Alberta and Saskatchewan on\n        April 30, 2010 and includes amendments to the Agreement."
                    }
                },
                {
                    "@id": "d2e33",
                    "bcl:marginalnote": "Prohibition on private cause of action",
                    "bcl:num": "2",
                    "bcl:subsection": [
                        {
                            "@id": "d2e41",
                            "bcl:num": "1",
                            "bcl:text": "Subject to subsection (2), no legal proceeding lies or may be brought or continued\n          against the government or another person to enforce or determine a right or obligation\n          that is claimed or arises solely under the Agreement or the Agreement as interpreted by a\n          joint decision issued under Article 34 (4) of the Agreement."
                        },
                        {
                            "@id": "d2e50",
                            "bcl:num": "2",
                            "bcl:text": "Subsection\n            (1) does not apply to a proceeding that is contemplated by Part IV of the\n          Agreement."
                        },
                        {
                            "@id": "d2e59",
                            "bcl:num": "3",
                            "bcl:text": "No legal proceeding lies or may be brought or continued against the government for\n          compensation, damages or any other remedy for anything arising as a consequence of a joint\n          decision issued under Article 34 (4) of the Agreement."
                        }
                    ]
                },
                {
                    "@id": "d2e69",
                    "bcl:marginalnote": "Assigning responsibilities of government",
                    "bcl:num": "3",
                    "bcl:text": "The Lieutenant Governor in Council by order may assign to the minister the\n        responsibility of",
                    "bcl:paragraph": [
                        {
                            "@id": "d2e80",
                            "bcl:num": "a",
                            "bcl:text": "carrying out on behalf of the government any of its powers or duties under the\n          Agreement, or"
                        },
                        {
                            "@id": "d2e89",
                            "bcl:num": "b",
                            "bcl:text": "exercising on behalf of the government any discretion it has under the\n          Agreement."
                        }
                    ]
                },
                {
                    "@id": "d2e99",
                    "bcl:marginalnote": "Repealed",
                    "bcl:num": "4",
                    "bcl:text": "[Repealed 2011-3-4 (3).]"
                },
