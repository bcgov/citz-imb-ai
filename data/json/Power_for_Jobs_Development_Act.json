{
    "act:act": {
        "@xmlns:act": "http://www.gov.bc.ca/2013/legislation/act",
        "@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
        "@xmlns:bcl": "http://www.gov.bc.ca/2013/bclegislation",
        "@xmlns:xlink": "http://www.w3.org/1999/xlink",
        "@xmlns:in": "http://www.qp.gov.bc.ca/2013/inline",
        "@xmlns:fun": "http://www.bclaws.ca/xslt/functions",
        "@xmlns:oasis": "http://docs.oasis-open.org/ns/oasis-exchange/table",
        "@xmlns:reg": "http://www.gov.bc.ca/2013/legislation/regulation",
        "@xsi:schemaLocation": "http://www.gov.bc.ca/2013/legislation/act http://standards.qp.gov.bc.ca/standards/2013/act_1.0.xsd",
        "@id": "00_97051_01",
        "act:title": "Power for Jobs Development Act",
        "act:chapter": "51",
        "act:yearenacted": "1997",
        "act:assentedto": "July 30, 1997",
        "act:currency": {
            "act:tlcDirPath": "1922970521/97051/tlc97051_f",
            "act:tlc": "E3tlc97051"
        },
        "act:content": {
            "bcl:part": [
                {
                    "@id": "d2e18",
                    "bcl:num": "1",
                    "bcl:text": "Introductory Provisions",
                    "bcl:section": [
                        {
                            "@id": "d2e27",
                            "bcl:marginalnote": "Definitions",
                            "bcl:num": "1",
                            "bcl:text": "In this Act:",
                            "bcl:definition": [
                                {
                                    "@id": "d2e38",
                                    "bcl:text": {
                                        "in:term": "administrator",
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96138_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Financial Administration Act"
                                        },
                                        "#text": "means an officer or employee of the ministry or of a government body, within the meaning of the , designated as the Power for Jobs Administrator by the minister;"
                                    }
                                },
                                {
                                    "@id": "d2e50",
                                    "bcl:text": {
                                        "in:term": "authority",
                                        "#text": "means the British Columbia Hydro and Power Authority and includes a subsidiary of the British Columbia Hydro and Power Authority;"
                                    }
                                },
                                {
                                    "@id": "d2e60",
                                    "bcl:text": {
                                        "in:term": "business",
                                        "#text": "means an individual, partnership, corporation or other organization carrying on, in British Columbia, a trade, enterprise, calling or undertaking or preparing to do so;"
                                    }
                                },
                                {
                                    "@id": "d2e69",
                                    "bcl:text": {
                                        "in:term": "Canadian entitlement",
                                        "#text": "means the Canadian share of downstream power benefits as determined under Article VII of the Treaty between Canada and the United States of America relating to Cooperative Development of the Water Resources of the Columbia River Basin;"
                                    }
                                },
                                {
                                    "@id": "d2e78",
                                    "bcl:text": {
                                        "in:term": "customer",
                                        "#text": "means a business that, for the operation of a plant or for any other economic activity, purchases electricity in an amount that exceeds 35 kW annually;"
                                    }
                                },
                                {
                                    "@id": "d2e87",
                                    "bcl:text": {
                                        "in:term": "development power rate",
                                        "#text": "means"
                                    },
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e96",
                                            "bcl:num": "a",
                                            "bcl:text": "a rate that is payable to the authority or the government by a business in respect of which an order is made under section 3 for one or both of surplus electricity and the delivery of that surplus electricity, or"
                                        },
                                        {
                                            "@id": "d2e105",
                                            "bcl:num": "b",
                                            "bcl:text": "a rate that is payable to a public utility or to a municipality or regional district by a business in respect of which an order is made under section 6 for one or both of surplus electricity and the delivery of that surplus electricity;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e115",
                                    "bcl:text": {
                                        "in:term": "Job Protection Commissioner",
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96240_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Job Protection Act"
                                        },
                                        "#text": "means the Job Protection Commissioner appointed under the ;"
                                    }
                                },
                                {
                                    "@id": "d2e127",
                                    "bcl:text": {
                                        "in:term": "public utility",
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96473_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Utilities Commission Act"
                                        },
                                        "#text": "has the same meaning as in the ;"
                                    }
                                },
                                {
                                    "@id": "d2e139",
                                    "bcl:text": {
                                        "in:term": "rate",
                                        "#text": "includes"
                                    },
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e148",
                                            "bcl:num": "a",
                                            "bcl:text": "a general, individual or joint rate, fare, toll, charge, rental or other compensation of a public utility or of the government,"
                                        },
                                        {
                                            "@id": "d2e157",
                                            "bcl:num": "b",
                                            "bcl:text": "a rule, practice, measurement, classification or contract of a public utility, the government or a corporation relating to a rate, and"
                                        },
                                        {
                                            "@id": "d2e166",
                                            "bcl:num": "c",
                                            "bcl:text": "a schedule or tariff respecting a rate;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e176",
                                    "bcl:text": {
                                        "in:term": "supply",
                                        "#text": "includes wheeling by the authority of surplus electricity that is a portion of the Canadian entitlement;"
                                    }
                                },
                                {
                                    "@id": "d2e185",
                                    "bcl:text": {
                                        "in:term": "surplus electricity",
                                        "#text": "means electricity that is one or both of the following:"
                                    },
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e194",
                                            "bcl:num": "a",
                                            "bcl:text": "surplus to the authority's requirements to supply",
                                            "bcl:subparagraph": [
                                                {
                                                    "@id": "d2e203",
                                                    "bcl:num": "i",
                                                    "bcl:text": "customers in the authority's service area, and"
                                                },
                                                {
                                                    "@id": "d2e212",
                                                    "bcl:num": "ii",
                                                    "bcl:text": {
                                                        "bcl:link": {
                                                            "@resource": "leg",
                                                            "@xlink:href": "/legislation/96473_01",
                                                            "@xlink:type": "locator",
                                                            "in:doc": "Utilities Commission Act"
                                                        },
                                                        "#text": "electricity in respect of which removal is permitted under the  and that is either"
                                                    },
                                                    "bcl:clause": [
                                                        {
                                                            "@id": "d2e224",
                                                            "bcl:num": "A",
                                                            "bcl:text": "produced at a hydroelectric generating facility owned by the authority, or"
                                                        },
                                                        {
                                                            "@id": "d2e234",
                                                            "bcl:num": "B",
                                                            "bcl:text": "purchased by the authority from another source;"
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "@id": "d2e243",
                                            "bcl:num": "b",
                                            "bcl:text": "a portion of the Canadian entitlement that the Lieutenant Governor in Council determines may be made available from time to time for the purpose of this Act."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e253",
                            "bcl:marginalnote": "Purpose",
                            "bcl:num": "2",
                            "bcl:text": "The purpose of this Act is to help ensure that British Columbia's electric power resources contribute to the creation and retention of jobs in British Columbia and to regional economic development."
                        }
                    ]
                },
                {
                    "@id": "d2e265",
                    "bcl:num": "2",
                    "bcl:text": "Provision of Electrical Power",
                    "bcl:section": [
                        {
                            "@id": "d2e274",
                            "bcl:marginalnote": "Development power rates",
                            "bcl:num": "3",
                            "bcl:text": "On application by any business and despite the rate charged by the authority for the supply of electricity to the business, the Lieutenant Governor in Council may order that surplus electricity be supplied to, and a development power rate applies to some or all of the supply of electricity to, that business if the application is consistent with the criteria prescribed under section 15 (2) (a) and if",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e285",
                                    "bcl:num": "a",
                                    "bcl:text": "the business proposes to construct or expand a plant or to expand operations in the authority's service area, the proposed plant or expansion will have the effect of expanding employment and the business would, after completion of the plant or expansion, fall within the definition of \"customer\", or"
                                },
                                {
                                    "@id": "d2e294",
                                    "bcl:num": "b",
                                    "bcl:text": "the business is a customer that requires financial assistance to maintain",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e304",
                                            "bcl:num": "i",
                                            "bcl:text": "the ongoing viability of the business's plant or operations, and"
                                        },
                                        {
                                            "@id": "d2e313",
                                            "bcl:num": "ii",
                                            "bcl:text": "the continued employment of persons."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e324",
                            "bcl:marginalnote": "Considerations of Lieutenant Governor in Council",
                            "bcl:num": "4",
                            "bcl:text": "Before making an order under section 3 or 6 in respect of a business, the Lieutenant Governor in Council may consider any recommendations made by the administrator in relation to the business's application."
                        },
                        {
                            "@id": "d2e336",
                            "bcl:marginalnote": "Terms of orders",
                            "bcl:num": "5",
                            "bcl:text": "An order in relation to a business under section 3 or 6 may contain terms that the Lieutenant Governor in Council considers advisable, including, without limitation, terms",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e347",
                                    "bcl:num": "a",
                                    "bcl:text": "specifying the amount of surplus electricity that is to be supplied to the business and the amount of that surplus electricity that is to be supplied at a development power rate,"
                                },
                                {
                                    "@id": "d2e356",
                                    "bcl:num": "b",
                                    "bcl:text": "setting the development power rate for the surplus electricity to be supplied to the business, and"
                                },
                                {
                                    "@id": "d2e366",
                                    "bcl:num": "c",
                                    "bcl:text": "specifying the period or periods during which the surplus electricity is to be supplied or the development power rate is to be applicable."
                                }
                            ]
                        },
                        {
                            "@id": "d2e376",
                            "bcl:marginalnote": "Customers outside the area of the authority",
                            "bcl:num": "6",
                            "bcl:subsection": [
                                {
                                    "@id": "d2e384",
                                    "bcl:num": "1",
                                    "bcl:text": "On application by a business, the Lieutenant Governor in Council may, by order, authorize the authority to dispose of surplus electricity to another public utility to enable the public utility to supply electricity to the business for the purpose of a plant or operations if",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e393",
                                            "bcl:num": "a",
                                            "bcl:text": "the plant is or the operations are located in the service area of that public utility, and"
                                        },
                                        {
                                            "@id": "d2e402",
                                            "bcl:num": "b",
                                            "bcl:text": "the business would be eligible for an order under section\u00a03 were the plant or operations located in the service area of the authority."
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e412",
                                    "bcl:num": "2",
                                    "bcl:text": "If electricity is supplied to a public utility under subsection (1), the Lieutenant Governor in Council may order that the public utility supply some or all of the electricity to that business at a development power rate."
                                },
                                {
                                    "@id": "d2e421",
                                    "bcl:num": "3",
                                    "bcl:text": "If a municipality or regional district supplies electricity to customers within its own boundaries, an order may be made under subsections (1) and (2) in respect of that service as though the municipality or regional district were a public utility referred to in subsection (1)."
                                }
                            ]
                        },
                        {
                            "@id": "d2e431",
                            "bcl:marginalnote": "Effect of development power rates",
                            "bcl:num": "7",
                            "bcl:subsection": [
                                {
                                    "@id": "d2e439",
                                    "bcl:num": "1",
                                    "bcl:text": "If there is a conflict between the rates of the authority and an order made under section\u00a03 or 6, the order prevails."
                                },
                                {
                                    "@id": "d2e448",
                                    "bcl:num": "2",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96473_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Utilities Commission Act"
                                        },
                                        "#text": "A development power rate that is payable in accordance with the terms of an order made under section\u00a03 or 6 is applicable, despite sections\u00a061\u00a0(3) and 63 and Part 5 of the ."
                                    }
                                },
                                {
                                    "@id": "d2e460",
                                    "bcl:num": "3",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96473_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Utilities Commission Act"
                                        },
                                        "#text": "The British Columbia Utilities Commission does not have jurisdiction under section\u00a058\u00a0(1) and (2), 59\u00a0(4), 64\u00a0(1) or Part 5 of the  in respect of a development power rate."
                                    }
                                },
                                {
                                    "@id": "d2e473",
                                    "bcl:num": "4",
                                    "bcl:text": "A development power rate",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e482",
                                            "bcl:num": "a",
                                            "bcl:text": "is lawful, enforceable and collectable, and"
                                        },
                                        {
                                            "@id": "d2e491",
                                            "bcl:num": "b",
                                            "bcl:text": {
                                                "bcl:link": {
                                                    "@resource": "leg",
                                                    "@xlink:href": "/legislation/96473_01",
                                                    "@xlink:type": "locator",
                                                    "in:doc": "Utilities Commission Act"
                                                },
                                                "#text": "is not a rate to which section 59 (1) of the  applies."
                                            }
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e503",
                                    "bcl:num": "5",
                                    "bcl:text": {
                                        "bcl:link": [
                                            {
                                                "@resource": "leg",
                                                "@xlink:href": "/legislation/96473_01",
                                                "@xlink:type": "locator",
                                                "in:doc": "Utilities Commission Act"
                                            },
                                            {
                                                "@resource": "leg",
                                                "@xlink:href": "/legislation/96473_01",
                                                "@xlink:type": "locator",
                                                "in:doc": "Utilities Commission Act"
                                            }
                                        ],
                                        "#text": "The government, the authority and any public utility are not subject to Part 3 or Part\u00a05 of the  in relation to any matter arising out of the supply of electricity with respect to which an order has been made under section\u00a03 or 6 of this Act, unless the Lieutenant Governor in Council orders that a provision of Part\u00a03 or Part\u00a05 of the  that the Lieutenant Governor in Council specifies applies with respect to that matter."
                                    }
                                },
                                {
                                    "@id": "d2e518",
                                    "bcl:num": "6",
                                    "bcl:text": [
                                        "If there is any conflict or inconsistency between",
                                        "the order made under section 6 (1) and (2) prevails."
                                    ],
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e527",
                                            "bcl:num": "a",
                                            "bcl:text": "an order made under section 6 (1) and (2) for the purposes of section 6 (3), and"
                                        },
                                        {
                                            "@id": "d2e537",
                                            "bcl:num": "b",
                                            "bcl:text": "either",
                                            "bcl:subparagraph": [
                                                {
                                                    "@id": "d2e546",
                                                    "bcl:num": "i",
                                                    "bcl:text": {
                                                        "in:desc": "[imposition of fees and charges]",
                                                        "bcl:link": {
                                                            "@resource": "leg",
                                                            "@xlink:href": "/legislation/r15001_00",
                                                            "@xlink:type": "locator",
                                                            "in:doc": "Local Government Act"
                                                        },
                                                        "#text": "section 397\u00a0 of the , or any bylaw made under that section, or"
                                                    }
                                                },
                                                {
                                                    "@id": "d2e561",
                                                    "bcl:num": "ii",
                                                    "bcl:text": {
                                                        "in:desc": [
                                                            "[municipal fees]",
                                                            "[parcel tax bylaw]"
                                                        ],
                                                        "bcl:link": {
                                                            "@resource": "leg",
                                                            "@xlink:href": "/legislation/03026_00",
                                                            "@xlink:type": "locator",
                                                            "in:doc": "Community Charter"
                                                        },
                                                        "#text": "section 194\u00a0 or 200  of the , or any bylaw made under those sections,"
                                                    }
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e582",
                                    "bcl:num": "7",
                                    "bcl:text": "The wholesale transmission rates otherwise payable by those customers of the authority that transmit power under published wholesale transmission rates must not be altered to compensate for",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e591",
                                            "bcl:num": "a",
                                            "bcl:text": "an order made under section 3 or 6, or"
                                        },
                                        {
                                            "@id": "d2e601",
                                            "bcl:num": "b",
                                            "bcl:text": "any reduction in the wholesale transmission revenue requirement resulting from that order."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e611",
                            "bcl:marginalnote": "Application to the administrator",
                            "bcl:num": "8",
                            "bcl:text": "A business wishing to be considered for an order under section\u00a03 or 6 must apply to the administrator for that order."
                        }
                    ]
                },
                {
                    "@id": "d2e623",
                    "bcl:num": "3",
                    "bcl:text": "Power for Jobs Administrator",
                    "bcl:section": [
                        {
                            "@id": "d2e633",
                            "bcl:marginalnote": "Administrator's mandate",
                            "bcl:num": "9",
                            "bcl:subsection": [
                                {
                                    "@id": "d2e641",
                                    "bcl:num": "1",
                                    "bcl:text": "If the Lieutenant Governor in Council determines that surplus electricity may be made available under this Act, the minister may notify the administrator of",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e650",
                                            "bcl:num": "a",
                                            "bcl:text": "the projected amount and duration of supply of that surplus electricity,"
                                        },
                                        {
                                            "@id": "d2e659",
                                            "bcl:num": "b",
                                            "bcl:text": "any criteria, additional to the application evaluation criteria prescribed under section 15 (2) (a), that the Lieutenant Governor in Council intends to use in assessing applications for that surplus electricity, and"
                                        },
                                        {
                                            "@id": "d2e669",
                                            "bcl:num": "c",
                                            "bcl:text": "the manner in which the assessment required under subsection (2) is to be completed."
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e678",
                                    "bcl:num": "2",
                                    "bcl:text": "After receiving notification under subsection (1), the administrator must assess if and to which businesses the surplus electricity should be provided."
                                },
                                {
                                    "@id": "d2e687",
                                    "bcl:num": "3",
                                    "bcl:text": "In conducting an assessment under subsection (2), the administrator,",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e696",
                                            "bcl:num": "a",
                                            "bcl:text": "if notified by the minister under subsection (1) (c) as to the manner in which the assessment is to be completed, must conduct the assessment in accordance with that manner, or"
                                        },
                                        {
                                            "@id": "d2e705",
                                            "bcl:num": "b",
                                            "bcl:text": "in any other case, may use any method that the administrator considers advisable to effect the assessment, including, without limitation, any one or more of the following:",
                                            "bcl:subparagraph": [
                                                {
                                                    "@id": "d2e714",
                                                    "bcl:num": "i",
                                                    "bcl:text": "advertising the amount of surplus electricity that may be made available by the Lieutenant Governor in Council;"
                                                },
                                                {
                                                    "@id": "d2e724",
                                                    "bcl:num": "ii",
                                                    "bcl:text": "preparing a request for proposals and distributing it to any businesses that the administrator considers appropriate;"
                                                },
                                                {
                                                    "@id": "d2e733",
                                                    "bcl:num": "iii",
                                                    "bcl:text": "using auction processes;"
                                                },
                                                {
                                                    "@id": "d2e742",
                                                    "bcl:num": "iv",
                                                    "bcl:text": "negotiating with businesses;"
                                                },
                                                {
                                                    "@id": "d2e751",
                                                    "bcl:num": "v",
                                                    "bcl:text": "conducting financial or other evaluations of solicited and unsolicited proposals."
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e760",
                                    "bcl:num": "4",
                                    "bcl:text": "If the administrator issues a request for proposals in respect of surplus electricity, the administrator must include in the request for proposals the additional criteria, if any, referred to in subsection (1) (b) that the Lieutenant Governor in Council intends to use in assessing proposals or other applications for that surplus electricity."
                                },
                                {
                                    "@id": "d2e769",
                                    "bcl:num": "5",
                                    "bcl:text": "After receiving a proposal or other application for the provision to a business of surplus electricity under this Act, whether or not that application is provided in response to an action of the Lieutenant Governor in Council or the administrator, the administrator must make recommendations to the minister as to whether the application should be accepted and may suggest terms that the administrator considers should be imposed on the supply of that surplus electricity."
                                },
                                {
                                    "@id": "d2e779",
                                    "bcl:num": "6",
                                    "bcl:text": "If a proposal or other application in respect of which recommendations are made under subsection (5) appears to the minister to be consistent with the criteria to be applied by the Lieutenant Governor in Council in assessing the application, the minister is to provide the application and the recommendations of the administrator to the Lieutenant Governor in Council for assessment."
                                },
                                {
                                    "@id": "d2e788",
                                    "bcl:num": "7",
                                    "bcl:text": "The minister is to notify the administrator as to the disposition of the application and the administrator must notify the applicant as to that disposition."
                                },
                                {
                                    "@id": "d2e797",
                                    "bcl:num": "8",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96240_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Job Protection Act"
                                        },
                                        "#text": "The administrator may, when invited to do so by the Job Protection Commissioner and authorized to do so by the minister, consult with the Job Protection Commissioner in respect of any economic plan made or proposed under the  and may, if it appears appropriate to do so, recommend to the Lieutenant Governor in Council that an order be made under section 3 or 6 of this Act in order to assist in the economic plan."
                                    }
                                }
                            ]
                        },
                        {
                            "@id": "d2e810",
                            "bcl:marginalnote": "Considerations administrator is to consider",
                            "bcl:num": "10",
                            "bcl:text": "In carrying out the mandate under section 9, the administrator is to give due consideration to economic, environmental and societal interests including, without limitation,",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e821",
                                    "bcl:num": "a",
                                    "bcl:text": "any new employment relationships that may be established as a result of this Act,"
                                },
                                {
                                    "@id": "d2e830",
                                    "bcl:num": "b",
                                    "bcl:text": "government objectives with respect to employment equity,"
                                },
                                {
                                    "@id": "d2e840",
                                    "bcl:num": "c",
                                    "bcl:text": "the economic importance of a business to British Columbia or to a region or locality within British Columbia, and"
                                },
                                {
                                    "@id": "d2e849",
                                    "bcl:num": "d",
                                    "bcl:text": "the purpose of this Act."
                                }
                            ]
                        },
                        {
                            "@id": "d2e859",
                            "bcl:marginalnote": "Confidentiality",
                            "bcl:num": "11",
                            "bcl:text": "The administrator and every other person who has custody of or access to records or information provided by a business in a proposal or other application provided to the administrator under this Act must not disclose the records or information to any person except insofar as disclosure is",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e870",
                                    "bcl:num": "a",
                                    "bcl:text": "necessary for the purposes of this Act,"
                                },
                                {
                                    "@id": "d2e879",
                                    "bcl:num": "b",
                                    "bcl:text": "required by a court proceeding relating to this Act, or"
                                },
                                {
                                    "@id": "d2e889",
                                    "bcl:num": "c",
                                    "bcl:text": "for the purposes of the compilation and publication of statistical information by the government."
                                }
                            ]
                        },
                        {
                            "@id": "d2e899",
                            "bcl:marginalnote": "No testimony in civil action",
                            "bcl:num": "12",
                            "bcl:text": "The administrator is not, in a civil action to which the administrator is not a party, required to testify or produce evidence about records or information obtained in the discharge of duties under this Act."
                        },
                        {
                            "@id": "d2e911",
                            "bcl:marginalnote": "Report to Lieutenant Governor in Council",
                            "bcl:num": "13",
                            "bcl:text": "The administrator, in each year, must make to the Lieutenant Governor in Council a report of the activities of the administrator for the preceding calendar year."
                        }
                    ]
                },
                {
                    "@id": "d2e923",
                    "bcl:num": "4",
                    "bcl:text": "General Provisions",
                    "bcl:section": [
                        {
                            "@id": "d2e933",
                            "bcl:marginalnote": "Offence",
                            "bcl:num": "14",
                            "bcl:subsection": [
                                {
                                    "@id": "d2e941",
                                    "bcl:num": "1",
                                    "bcl:text": "A person who contravenes section 11 commits an offence."
                                },
                                {
                                    "@id": "d2e950",
                                    "bcl:num": "2",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96338_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Offence Act"
                                        },
                                        "#text": "Section 5 of the  does not apply in respect of this Act or the regulations."
                                    }
                                }
                            ]
                        },
                        {
                            "@id": "d2e963",
                            "bcl:marginalnote": "Power to make regulations",
                            "bcl:num": "15",
                            "bcl:subsection": [
                                {
                                    "@id": "d2e971",
                                    "bcl:num": "1",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96238_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Interpretation Act"
                                        },
                                        "#text": "The Lieutenant Governor in Council may make regulations referred to in section\u00a041 of the ."
                                    }
                                },
                                {
                                    "@id": "d2e983",
                                    "bcl:num": "2",
                                    "bcl:text": "Without limiting subsection (1), the Lieutenant Governor in Council may make regulations",
                                    "bcl:paragraph": [
                                        {
                                            "@id": "d2e992",
                                            "bcl:num": "a",
                                            "bcl:text": "respecting the criteria that may be applied by the Lieutenant Governor in Council in assessing a proposal or other application for an order under section 3 or 6, including, without limitation, the tests that may be applied by the Lieutenant Governor in Council in determining if and to what extent an order may be made under those sections, and"
                                        },
                                        {
                                            "@id": "d2e1002",
                                            "bcl:num": "b",
                                            "bcl:text": "prescribing the information that must be contained in a proposal or other application under section\u00a03 or 6."
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ],
            "bcl:section": {
                "@id": "d2e1012",
                "bcl:marginalnote": "Commencement",
                "bcl:num": "16",
                "bcl:text": "This Act comes into force by regulation of the Lieutenant Governor in Council."
            }
        }
    }
}