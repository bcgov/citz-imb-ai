{
    "act:act": {
        "@xmlns:act": "http://www.gov.bc.ca/2013/legislation/act",
        "@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
        "@xmlns:bcl": "http://www.gov.bc.ca/2013/bclegislation",
        "@xmlns:xlink": "http://www.w3.org/1999/xlink",
        "@xmlns:in": "http://www.qp.gov.bc.ca/2013/inline",
        "@xmlns:fun": "http://www.bclaws.ca/xslt/functions",
        "@xmlns:oasis": "http://docs.oasis-open.org/ns/oasis-exchange/table",
        "@xmlns:reg": "http://www.gov.bc.ca/2013/legislation/regulation",
        "@xsi:schemaLocation": "http://www.gov.bc.ca/2013/legislation/act http://standards.qp.gov.bc.ca/standards/2013/act_1.0.xsd",
        "@id": "00_96271_01",
        "act:title": "Livestock Identification Act",
        "act:chapter": "271",
        "act:yearenacted": "1996",
        "act:currency": {
            "act:tlcDirPath": "76470131/96271/tlc96271_f",
            "act:tlc": "E3tlc96271"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e15",
                    "bcl:marginalnote": "Definitions",
                    "bcl:num": "1",
                    "bcl:text": "In this Act:",
                    "bcl:definition": [
                        {
                            "@id": "d2e26",
                            "bcl:text": {
                                "in:term": "brand",
                                "#text": "means an identification impressed or affixed on or within the body of livestock to indicate ownership;"
                            }
                        },
                        {
                            "@id": "d2e35",
                            "bcl:text": {
                                "in:term": [
                                    "buy",
                                    "sell"
                                ],
                                "#text": "or  includes offering to do so;"
                            }
                        },
                        {
                            "@id": "d2e48",
                            "bcl:text": {
                                "in:term": "game",
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/14016",
                                    "@xlink:type": "locator",
                                    "in:doc": "Animal Health Act"
                                },
                                "#text": "has the same meaning as in a regulation made under the ;"
                            }
                        },
                        {
                            "@id": "d2e60",
                            "bcl:text": {
                                "in:term": "hide",
                                "#text": "means the untanned skin of livestock;"
                            }
                        },
                        {
                            "@id": "d2e69",
                            "bcl:text": {
                                "in:term": "hide dealer",
                                "#text": "means a person who buys or sells hides or possesses hides for sale;"
                            }
                        },
                        {
                            "@id": "d2e78",
                            "bcl:text": {
                                "in:term": "identification brand",
                                "#text": "means an identification impressed or affixed on or within the body of livestock for a purpose other than to indicate ownership;"
                            }
                        },
                        {
                            "@id": "d2e87",
                            "bcl:text": {
                                "in:term": "inspector",
                                "#text": "means an inspector appointed under section\u00a03 insofar as the inspector may act within the powers, functions, terms and conditions of the inspector's appointment under section\u00a03;"
                            }
                        },
                        {
                            "@id": "d2e96",
                            "bcl:text": {
                                "in:term": "livestock",
                                "#text": "means cattle, horses and game and includes other animals designated by regulation as livestock for the purposes of all or part of this Act or the regulations;"
                            }
                        },
                        {
                            "@id": "d2e106",
                            "bcl:text": {
                                "in:term": "livestock dealer",
                                "#text": "means a person who, directly or as an agent, buys or sells livestock or possesses livestock for sale but does not include a person who resides in British Columbia or Alberta and who transacts business that arises directly from the person's ranching or farming operation;"
                            }
                        },
                        {
                            "@id": "d2e115",
                            "bcl:text": {
                                "in:term": "meat",
                                "#text": "means the whole or part of a carcass of livestock but does not include the hide;"
                            }
                        },
                        {
                            "@id": "d2e124",
                            "bcl:text": {
                                "in:term": "register",
                                "#text": "means a register of brands for the purposes of this Act;"
                            }
                        },
                        {
                            "@id": "d2e133",
                            "bcl:text": {
                                "in:term": "registered brand",
                                "#text": "means a brand registered under this Act;"
                            }
                        },
                        {
                            "@id": "d2e142",
                            "bcl:text": {
                                "in:term": "registered owner",
                                "#text": "means a person in whose name a brand is registered;"
                            }
                        },
                        {
                            "@id": "d2e151",
                            "bcl:text": {
                                "in:term": "slaughter",
                                "#text": "means the killing of livestock for any purpose and includes the butchering and dressing of a carcass;"
                            }
                        },
                        {
                            "@id": "d2e161",
                            "bcl:text": {
                                "in:term": "slaughterhouse",
                                "#text": "means a building or place where livestock is slaughtered or held for slaughter."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e171",
                    "bcl:marginalnote": "Use of registered brand",
                    "bcl:num": "2",
                    "bcl:text": "A registered brand is the property of the registered owner who has the exclusive right to its use."
                },
                {
                    "@id": "d2e183",
                    "bcl:marginalnote": "Inspectors",
                    "bcl:num": "3",
                    "bcl:subsection": [
                        {
                            "@id": "d2e191",
                            "bcl:num": "1",
                            "bcl:text": "The minister may appoint a person, or persons within a class, to be inspectors."
                        },
                        {
                            "@id": "d2e200",
                            "bcl:num": "2",
                            "bcl:text": "The minister, in an appointment under subsection (1), may",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e209",
                                    "bcl:num": "a",
                                    "bcl:text": "limit the powers and functions under section\u00a05 that the inspector may exercise, or"
                                },
                                {
                                    "@id": "d2e219",
                                    "bcl:num": "b",
                                    "bcl:text": "make the appointment subject to terms and conditions."
                                }
                            ]
                        },
                        {
                            "@id": "d2e228",
                            "bcl:num": "3",
                            "bcl:text": "An officer or constable of the Royal Canadian Mounted Police or of any police force of a municipality has the powers of an inspector under section 5."
                        }
                    ]
                },
                {
                    "@id": "d2e238",
                    "bcl:marginalnote": "Evidence of appointment",
                    "bcl:num": "4",
                    "bcl:text": "The production by an inspector of a notice of appointment purported to be signed by the minister is proof in the absence of evidence to the contrary of the facts stated in the notice."
                },
                {
                    "@id": "d2e250",
                    "bcl:marginalnote": "Complaints",
                    "bcl:num": "4.1",
                    "bcl:subsection": [
                        {
                            "@id": "d2e258",
                            "bcl:num": "1",
                            "bcl:text": "A person affected by the exercise of a power under section\u00a05, or the refusal to exercise a power, by an inspector may complain to the body established under the regulations to investigate complaints."
                        },
                        {
                            "@id": "d2e267",
                            "bcl:num": "2",
                            "bcl:text": "On receipt of a complaint, the body must investigate the matter raised by the complaint and may request that the inspector who is the subject of the complaint provide it with any information regarding the matter that the inspector considers should be considered during the investigation."
                        },
                        {
                            "@id": "d2e276",
                            "bcl:num": "3",
                            "bcl:text": "The body referred to in subsection (1) may discontinue, at any stage, an investigation under this section if it considers the complaint to be unfounded, frivolous or vexatious."
                        },
                        {
                            "@id": "d2e286",
                            "bcl:num": "4",
                            "bcl:text": "Unless subsection (3) applies, after completing an investigation and considering the information, if any, provided by the inspector, the body must",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e295",
                                    "bcl:num": "a",
                                    "bcl:text": "prepare its report to the minister,"
                                },
                                {
                                    "@id": "d2e304",
                                    "bcl:num": "b",
                                    "bcl:text": "give the inspector an opportunity to respond to the report,"
                                },
                                {
                                    "@id": "d2e313",
                                    "bcl:num": "c",
                                    "bcl:text": "make alterations to the report in light of that response, where appropriate, and"
                                },
                                {
                                    "@id": "d2e322",
                                    "bcl:num": "d",
                                    "bcl:text": "submit the report to the minister."
                                }
                            ]
                        },
                        {
                            "@id": "d2e331",
                            "bcl:num": "5",
                            "bcl:text": "On receipt of a report made under subsection (4), the minister may, as the minister considers appropriate,",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e341",
                                    "bcl:num": "a",
                                    "bcl:text": "rescind the inspector's appointment,"
                                },
                                {
                                    "@id": "d2e350",
                                    "bcl:num": "b",
                                    "bcl:text": "add terms or conditions, or limit powers or functions, as described in section\u00a03\u00a0(2) respecting the appointment, or"
                                },
                                {
                                    "@id": "d2e359",
                                    "bcl:num": "c",
                                    "bcl:text": "take no action."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e370",
                    "bcl:marginalnote": "Power of inspectors",
                    "bcl:num": "5",
                    "bcl:subsection": [
                        {
                            "@id": "d2e378",
                            "bcl:num": "1",
                            "bcl:text": "For the purpose of carrying out duties under this Act an inspector may do any of the following:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e387",
                                    "bcl:num": "a",
                                    "bcl:text": "after taking reasonable steps to notify the owner or occupant or agent of either of them, enter",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e396",
                                            "bcl:num": "i",
                                            "bcl:text": "land or a vehicle, or"
                                        },
                                        {
                                            "@id": "d2e406",
                                            "bcl:num": "ii",
                                            "bcl:text": "during normal business hours, a building other than a building or part of a building that is used for residential purposes;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e415",
                                    "bcl:num": "b",
                                    "bcl:text": "inspect livestock, a carcass or a hide for evidence of a brand, ownership or purchase;"
                                },
                                {
                                    "@id": "d2e424",
                                    "bcl:num": "c",
                                    "bcl:text": [
                                        "stop a vehicle the inspector believes contains livestock, meat or hides and, if the inspector",
                                        "detain a shipment of livestock, meat or hides, and the vehicle containing it, at the owner's risk and expense, and direct the shipment and the vehicle to a place the inspector designates;"
                                    ],
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e433",
                                            "bcl:num": "i",
                                            "bcl:text": "believes this Act or the regulations are being contravened, or"
                                        },
                                        {
                                            "@id": "d2e442",
                                            "bcl:num": "ii",
                                            "bcl:text": "is not satisfied about the ownership of the livestock, meat or hides,"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e455",
                                    "bcl:num": "d",
                                    "bcl:text": {
                                        "bcl:link": [
                                            {
                                                "@resource": "leg",
                                                "@xlink:href": "/legislation/14016",
                                                "@xlink:type": "locator",
                                                "in:doc": "Animal Health Act"
                                            },
                                            {
                                                "@resource": "leg",
                                                "@xlink:href": "http://laws-lois.justice.gc.ca/eng/acts/H-3.3/index.html",
                                                "@xlink:type": "locator",
                                                "in:doc": "Health of Animals Act"
                                            }
                                        ],
                                        "#text": "if the inspector believes livestock is being transported contrary to the  or the  (Canada),"
                                    },
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e470",
                                            "bcl:num": "i",
                                            "bcl:text": "stop and detain the livestock and a vehicle containing it, at the owner's risk and expense, and"
                                        },
                                        {
                                            "@id": "d2e479",
                                            "bcl:num": "ii",
                                            "bcl:text": "direct the livestock and the vehicle to a place the inspector designates;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e488",
                                    "bcl:num": "e",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96318_00",
                                            "@xlink:type": "locator",
                                            "in:doc": "Motor Vehicle Act"
                                        },
                                        "#text": "despite the , operate a vehicle equipped with a flashing light or siren, or both;"
                                    }
                                },
                                {
                                    "@id": "d2e500",
                                    "bcl:num": "f",
                                    "bcl:text": "seize and remove livestock, meat or hides if",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e509",
                                            "bcl:num": "i",
                                            "bcl:text": "the inspector believes they are being kept, driven, removed or shipped contrary to this Act or another enactment of British Columbia or Canada, or"
                                        },
                                        {
                                            "@id": "d2e519",
                                            "bcl:num": "ii",
                                            "bcl:text": "the inspector is not satisfied about the ownership;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e528",
                                    "bcl:num": "g",
                                    "bcl:text": "take into custody livestock that appears to be abandoned;"
                                },
                                {
                                    "@id": "d2e537",
                                    "bcl:num": "h",
                                    "bcl:text": "in accordance with the regulations, dispose of livestock, meat or hides that have been seized, detained or otherwise taken into custody;"
                                },
                                {
                                    "@id": "d2e546",
                                    "bcl:num": "i",
                                    "bcl:text": "require records relating to livestock, meat or a hide to be presented for inspection."
                                }
                            ]
                        },
                        {
                            "@id": "d2e555",
                            "bcl:num": "2",
                            "bcl:text": "The Lieutenant Governor in Council may prescribe additional powers to be exercised by an inspector."
                        },
                        {
                            "@id": "d2e564",
                            "bcl:num": "3",
                            "bcl:text": "Nothing in this section affects the right of a person to take action in respect of a negligent act or omission of a person acting under this section."
                        }
                    ]
                },
                {
                    "@id": "d2e574",
                    "bcl:marginalnote": "Repealed",
                    "bcl:num": "6\u20137",
                    "bcl:text": "[Repealed 1997-14-15.]"
                },
                {
                    "@id": "d2e586",
                    "bcl:marginalnote": "Evidence",
                    "bcl:num": "8",
                    "bcl:subsection": [
                        {
                            "@id": "d2e594",
                            "bcl:num": "1",
                            "bcl:text": "A certificate signed by an inspector respecting a matter or thing for which the inspector is responsible under this Act is evidence of the facts stated in the certificate concerning the matter or thing."
                        },
                        {
                            "@id": "d2e603",
                            "bcl:num": "2",
                            "bcl:text": "An unexpired brand certificate is evidence that the person indicated on the certificate is the registered owner."
                        },
                        {
                            "@id": "d2e612",
                            "bcl:num": "3",
                            "bcl:text": "A brand on livestock is evidence that the registered owner of the brand is the owner of the livestock."
                        },
                        {
                            "@id": "d2e622",
                            "bcl:num": "4",
                            "bcl:text": "In a prosecution under this Act, a bill of sale made under a requirement of this Act or the regulations is evidence that the livestock described in the bill of sale was bought by the buyer or owner named in the bill of sale."
                        }
                    ]
                },
                {
                    "@id": "d2e632",
                    "bcl:marginalnote": "Offences",
                    "bcl:num": "9",
                    "bcl:subsection": [
                        {
                            "@id": "d2e640",
                            "bcl:num": "1",
                            "bcl:text": "A person must not do any of the following:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e649",
                                    "bcl:num": "a",
                                    "bcl:text": "brand livestock with",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e658",
                                            "bcl:num": "i",
                                            "bcl:text": "an unregistered brand,"
                                        },
                                        {
                                            "@id": "d2e668",
                                            "bcl:num": "ii",
                                            "bcl:text": "a brand other than that of the owner of the livestock, or"
                                        },
                                        {
                                            "@id": "d2e677",
                                            "bcl:num": "iii",
                                            "bcl:text": "an identification brand not authorized by an organization designated by an order under section 9.1;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e686",
                                    "bcl:num": "b",
                                    "bcl:text": "unlawfully possess livestock branded with",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e695",
                                            "bcl:num": "i",
                                            "bcl:text": "an unregistered brand,"
                                        },
                                        {
                                            "@id": "d2e704",
                                            "bcl:num": "ii",
                                            "bcl:text": "an identification brand not authorized by an organization designated by an order under section\u00a09.1, or"
                                        },
                                        {
                                            "@id": "d1e735",
                                            "bcl:num": "iii",
                                            "bcl:text": "a brand other than the livestock owner's brand;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e713",
                                    "bcl:num": "c",
                                    "bcl:text": "alter, deface or obliterate a brand or identification brand on livestock or a hide;"
                                },
                                {
                                    "@id": "d2e723",
                                    "bcl:num": "d",
                                    "bcl:text": "at the request of an inspector, refuse to stop a vehicle under the person's control or otherwise disobey a direction or order given by an inspector under this Act;"
                                },
                                {
                                    "@id": "d2e732",
                                    "bcl:num": "e) to (g",
                                    "bcl:text": "[Repealed 1997-14-17.]"
                                },
                                {
                                    "@id": "d2e741",
                                    "bcl:num": "h",
                                    "bcl:text": "wilfully hinder or interfere with an inspector in the performance of the inspector's duty;"
                                },
                                {
                                    "@id": "d2e750",
                                    "bcl:num": "i",
                                    "bcl:text": "present livestock for inspection without authority of the owner;"
                                },
                                {
                                    "@id": "d2e759",
                                    "bcl:num": "j",
                                    "bcl:text": "describe livestock or brands incorrectly on a bill of sale."
                                }
                            ]
                        },
                        {
                            "@id": "d2e768",
                            "bcl:num": "2",
                            "bcl:text": "A person who contravenes subsection (1) commits an offence and is liable on conviction to a fine of not more than $2 000."
                        },
                        {
                            "@id": "d2e778",
                            "bcl:num": "3) and (4",
                            "bcl:text": "[Repealed 1997-14-17.]"
                        }
                    ]
                },
                {
                    "@id": "d2e788",
                    "bcl:marginalnote": "Registration programs",
                    "bcl:num": "9.1",
                    "bcl:subsection": [
                        {
                            "@id": "d2e796",
                            "bcl:num": "1",
                            "bcl:text": "The minister, by order, may",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e805",
                                    "bcl:num": "a",
                                    "bcl:text": "designate organizations that may establish one or more programs specified in the order for the registration of brands or use of identification brands for British Columbia or an area of British Columbia, and"
                                },
                                {
                                    "@id": "d2e814",
                                    "bcl:num": "b",
                                    "bcl:text": "set the terms and conditions that an organization designated under paragraph (a) must follow",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e824",
                                            "bcl:num": "i",
                                            "bcl:text": "to establish and administer a program for the registration of brands or use of identification brands, or"
                                        },
                                        {
                                            "@id": "d2e833",
                                            "bcl:num": "ii",
                                            "bcl:text": "to issue, refuse to issue, attach conditions to, transfer, amend, renew, suspend or cancel the registration of a brand or use of an identification brand."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e842",
                            "bcl:num": "2",
                            "bcl:text": "An order under subsection (1) may",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e851",
                                    "bcl:num": "a",
                                    "bcl:text": "make different provisions for different areas of British Columbia, organizations, categories of livestock, brands or identification brands,"
                                },
                                {
                                    "@id": "d2e860",
                                    "bcl:num": "b",
                                    "bcl:text": "specify the circumstances under which and the extent to which an organization may delegate one or more of its powers, duties or functions under a program to its officers, agents or employees, or"
                                },
                                {
                                    "@id": "d2e869",
                                    "bcl:num": "c",
                                    "bcl:text": "authorize and require an organization to",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e879",
                                            "bcl:num": "i",
                                            "bcl:text": "maintain livestock that is awaiting inspection or is detained by an inspector, and"
                                        },
                                        {
                                            "@id": "d2e888",
                                            "bcl:num": "ii",
                                            "bcl:text": "detain and sell livestock that is abandoned."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e897",
                            "bcl:num": "3",
                            "bcl:text": "An organization designated under subsection (1) may charge and retain fees for services it performs."
                        }
                    ]
                },
                {
                    "@id": "d2e907",
                    "bcl:marginalnote": "Direction by the minister",
                    "bcl:num": "9.2",
                    "bcl:subsection": [
                        {
                            "@id": "d2e915",
                            "bcl:num": "1",
                            "bcl:text": "The minister may issue a direction to an organization designated under section\u00a09.1 or to its members specifying the factors, criteria and guidelines that the organization or members must or must not use in exercising the organization's powers."
                        },
                        {
                            "@id": "d2e924",
                            "bcl:num": "2",
                            "bcl:text": "The organization and its members must comply with any general or special direction made by the minister under subsection (1)."
                        }
                    ]
                },
                {
                    "@id": "d2e935",
                    "bcl:marginalnote": "Power to make regulations",
                    "bcl:num": "10",
                    "bcl:subsection": [
                        {
                            "@id": "d2e943",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96238_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Interpretation Act"
                                },
                                "#text": "The Lieutenant Governor in Council may make regulations referred to in section\u00a041 of the ."
                            }
                        },
                        {
                            "@id": "d2e955",
                            "bcl:num": "2",
                            "bcl:text": "Without limiting subsection (1), the Lieutenant Governor in Council may make regulations respecting the following:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e964",
                                    "bcl:num": "a",
                                    "bcl:text": "[Repealed 1997-14-19.]"
                                },
                                {
                                    "@id": "d2e974",
                                    "bcl:num": "b",
                                    "bcl:text": "the types or classes of livestock that must be branded to be within an area of British Columbia specified in the regulation;"
                                },
                                {
                                    "@id": "d2e983",
                                    "bcl:num": "c",
                                    "bcl:text": "the duties of inspectors;"
                                },
                                {
                                    "@id": "d2e992",
                                    "bcl:num": "d",
                                    "bcl:text": "[Repealed 1997-14-19.]"
                                },
                                {
                                    "@id": "d2e1001",
                                    "bcl:num": "e",
                                    "bcl:text": "dealings in livestock and hides;"
                                },
                                {
                                    "@id": "d2e1010",
                                    "bcl:num": "f",
                                    "bcl:text": "brand inspection of livestock and hides;"
                                },
                                {
                                    "@id": "d2e1019",
                                    "bcl:num": "g",
                                    "bcl:text": "movement of livestock, hides and meat;"
                                },
                                {
                                    "@id": "d2e1029",
                                    "bcl:num": "h",
                                    "bcl:text": "[Repealed 1997-14-19.]"
                                },
                                {
                                    "@id": "d2e1038",
                                    "bcl:num": "i",
                                    "bcl:text": "preservation of brand marks, ear tags, back tags or other livestock identification systems and of the hide of the livestock carcass for the purpose of inspection;"
                                },
                                {
                                    "@id": "d2e1047",
                                    "bcl:num": "j",
                                    "bcl:text": "particulars of brands or other information or records to be delivered, kept or obtained on a sale, delivery or slaughter of livestock or on a sale of hides;"
                                },
                                {
                                    "@id": "d2e1056",
                                    "bcl:num": "k",
                                    "bcl:text": "records to be kept and made available by a person slaughtering livestock or dealing in livestock, hides or meat;"
                                },
                                {
                                    "@id": "d2e1065",
                                    "bcl:num": "l",
                                    "bcl:text": "the exemption of a person or class of persons from all or part of this Act or the regulations;"
                                },
                                {
                                    "@id": "d2e1074",
                                    "bcl:num": "m",
                                    "bcl:text": "[Repealed 1997-14-19.]"
                                },
                                {
                                    "@id": "d2e1084",
                                    "bcl:num": "n) and (o",
                                    "bcl:text": "[Repealed 1997-14-19.]"
                                },
                                {
                                    "@id": "d2e1093",
                                    "bcl:num": "p",
                                    "bcl:text": "rights of a person to inspect livestock being driven on land other than land of the livestock owner;"
                                },
                                {
                                    "@id": "d2e1102",
                                    "bcl:num": "q",
                                    "bcl:text": "responsibilities of a person who while transporting livestock finds livestock belonging to another person mixed with the person's own livestock;"
                                },
                                {
                                    "@id": "d2e1111",
                                    "bcl:num": "r",
                                    "bcl:text": "inspection of livestock by an inspector and the issue of an inspection certificate;"
                                },
                                {
                                    "@id": "d2e1120",
                                    "bcl:num": "s",
                                    "bcl:text": "the establishment of a body for the purposes of section\u00a04.1, the procedures to be followed by the body when conducting an investigation into a complaint and requesting information from the investigator who is the subject of the complaint, and the information to be contained in the report by the body."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e1130",
                    "bcl:marginalnote": "Revocation of brand registration",
                    "bcl:num": "11",
                    "bcl:subsection": [
                        {
                            "@id": "d2e1138",
                            "bcl:num": "1",
                            "bcl:text": "On the date this section comes into force",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e1147",
                                    "bcl:num": "a",
                                    "bcl:text": "all registrations, by the Recorder of Brands under this Act, of brands and use of identification brands are revoked, and"
                                },
                                {
                                    "@id": "d2e1156",
                                    "bcl:num": "b",
                                    "bcl:text": "brand certificates, issued by the Recorder of Brands under this Act, have no force or effect on and after that date except for the purposes of section 8 respecting matters arising before that date."
                                }
                            ]
                        },
                        {
                            "@id": "d2e1166",
                            "bcl:num": "2",
                            "bcl:text": "Despite any other enactment, no damages or compensation of any kind is payable by the government, and no proceedings in which damages or compensation is claimed may be commenced, as a consequence of subsection (1)."
                        }
                    ]
                }
            ]
        }
    }
}