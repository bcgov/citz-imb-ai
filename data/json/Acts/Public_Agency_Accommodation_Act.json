        "@id": "00_06007_01",
        "act:title": "Public Agency Accommodation Act",
        "act:chapter": "7",
        "act:yearenacted": "2006",
        "act:assentedto": "March 28, 2006",
        "act:currency": {
            "act:tlcDirPath": "1922970521/06007/tlc06007_f",
            "act:tlc": "E3tlc06007"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e18",
                    "bcl:marginalnote": "Definitions",
                    "bcl:num": "1",
                    "bcl:text": "In this Act:",
                    "bcl:definition": [
                        {
                            "@id": "d2e29",
                            "bcl:text": {
                                "in:term": "administered land",
                                "#text": "means land referred to in section 6 (1) (a), (b), (c) or (d) while under the minister's administration;"
                            }
                        },
                        {
                            "@id": "d2e41",
                            "bcl:text": {
                                "in:term": "corporation",
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96033REP_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "British Columbia Buildings Corporation Act"
                                },
                                "#text": "means the British Columbia Buildings Corporation continued under section 2 (1) of the ;"
                            }
                        },
                        {
                            "@id": "d2e54",
                            "bcl:text": {
                                "in:term": "public agency",
                                "#text": "means"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e63",
                                    "bcl:num": "a",
                                    "bcl:text": "the government, including, without limitation, a ministry, office, agency, board or commission of the government,"
                                },
                                {
                                    "@id": "d2e72",
                                    "bcl:num": "b",
                                    "bcl:text": "the government of Canada, including, without limitation, a ministry, office, agency, board or commission of that government,"
                                },
                                {
                                    "@id": "d2e81",
                                    "bcl:num": "c",
                                    "bcl:text": "a local government,"
                                },
                                {
                                    "@id": "d2e90",
                                    "bcl:num": "d",
                                    "bcl:text": "the Legislative Assembly, including, without limitation, the Legislative Library, the office of any officer of the Legislature and the office of any officer of the Legislative Assembly,"
                                },
                                {
                                    "@id": "d2e99",
                                    "bcl:num": "e",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96138_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Financial Administration Act"
                                        },
                                        "#text": "a government body within the meaning of the , or"
                                    }
                                },
                                {
                                    "@id": "d2e112",
                                    "bcl:num": "f",
                                    "bcl:text": "any other entity designated as a public agency for the purposes of this Act by a directive of Treasury Board."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e122",
                    "bcl:marginalnote": "Transfer of property",
                    "bcl:num": "2",
                    "bcl:subsection": [
                        {
                            "@id": "d2e130",
                            "bcl:num": "1",
                            "bcl:text": "Despite any enactment or law to the contrary, on the coming into force of this Act, all of the corporation's rights, property and assets are transferred to and vested in the government."
                        },
                        {
                            "@id": "d2e139",
                            "bcl:num": "2",
                            "bcl:text": "Despite any other enactment or law to the contrary,",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e148",
                                    "bcl:num": "a",
                                    "bcl:text": "the transfer and vesting effected by subsection (1) takes effect without",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e158",
                                            "bcl:num": "i",
                                            "bcl:text": "the execution or issue of any record, or"
                                        },
                                        {
                                            "@id": "d2e167",
                                            "bcl:num": "ii",
                                            "bcl:text": "any registration or filing of this Act or any other record in or with any registry or other office,"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e176",
                                    "bcl:num": "b",
                                    "bcl:text": "the transfer and vesting effected by subsection (1) takes effect despite",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e185",
                                            "bcl:num": "i",
                                            "bcl:text": "any prohibition on all or any part of the transfer, and"
                                        },
                                        {
                                            "@id": "d2e194",
                                            "bcl:num": "ii",
                                            "bcl:text": "the absence of any consent or approval that is or may be required for all or any part of the transfer,"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e203",
                                    "bcl:num": "c",
                                    "bcl:text": "if any right, property or asset referred to in subsection (1) is registered or otherwise recorded in the name of the corporation, that registration or record may remain but is deemed, for all purposes of this and all other enactments and law, to reflect that the right, property or asset is owned by and vested in the government, and"
                                },
                                {
                                    "@id": "d2e213",
                                    "bcl:num": "d",
                                    "bcl:text": "in any record in or by which the government deals with a right, property or asset referred to in subsection (1), it is sufficient to cite this Act as effecting and confirming the transfer from the corporation to the government of title to the right, property or asset and the vesting of that title in the government."
                                }
                            ]
                        },
                        {
                            "@id": "d2e222",
                            "bcl:num": "3",
                            "bcl:text": "For the purposes of this section, assets that become assets of the government under this section include records and parts of records, and, without limiting this, all of the records and parts of records of the corporation are transferred to and become the records of the government on the coming into force of this Act."
                        }
                    ]
                },
                {
                    "@id": "d2e232",
                    "bcl:marginalnote": "Transfer of obligations and liabilities",
                    "bcl:num": "3",
                    "bcl:text": "On the coming into force of this Act, all obligations and liabilities of the corporation",
                    "bcl:paragraph": [
                        {
                            "@id": "d2e243",
                            "bcl:num": "a",
                            "bcl:text": "are transferred to and assumed by the government,"
                        },
                        {
                            "@id": "d2e252",
                            "bcl:num": "b",
                            "bcl:text": "become the government's obligations and liabilities, and"
                        },
                        {
                            "@id": "d2e262",
                            "bcl:num": "c",
                            "bcl:text": "may be enforced against the government as if the government had incurred them."
                        }
                    ]
                },
                {
                    "@id": "d2e272",
                    "bcl:marginalnote": "Records of transferred assets and liabilities",
                    "bcl:num": "4",
                    "bcl:subsection": [
                        {
                            "@id": "d2e280",
                            "bcl:num": "1",
                            "bcl:text": "Subject to subsection (2), a reference to the corporation in any record, including, without limitation, in any security agreement, commercial paper, lease, licence, permit or other contract, instrument, document or certificate that creates, evidences or otherwise relates to a right, property, asset, obligation or liability transferred to the government under this Act, is deemed to be a reference to the government."
                        },
                        {
                            "@id": "d2e289",
                            "bcl:num": "2",
                            "bcl:text": "If, under this Act, a part of a right, property, asset, obligation or liability is transferred to the government, any record, including, without limitation, any security agreement, commercial paper, lease, licence, permit or other contract, instrument, document or certificate that creates, evidences or otherwise relates to that right, property, asset, obligation or liability, is deemed to be amended to reflect the government's interests in that right, property, asset, obligation or liability."
                        }
                    ]
                },
                {
                    "@id": "d2e299",
                    "bcl:marginalnote": "Dealings with transferred assets",
                    "bcl:num": "5",
                    "bcl:text": {
                        "bcl:link": {
                            "@resource": "leg",
                            "@xlink:href": "/legislation/96250_00",
                            "@xlink:type": "locator",
                            "in:doc": "Land Title Act"
                        },
                        "#text": "Without limiting section 2 (2) (c) or section 383.1 of the , if a right, property or asset referred to in section 2 (1) is registered or recorded in the name of the corporation,"
                    },
                    "bcl:paragraph": [
                        {
                            "@id": "d2e313",
                            "bcl:num": "a",
                            "bcl:text": "the government may, in its own name,",
                            "bcl:subparagraph": [
                                {
                                    "@id": "d2e322",
                                    "bcl:num": "i",
                                    "bcl:text": "effect a transfer, charge, encumbrance or other dealing with the right, property or asset, and"
                                },
                                {
                                    "@id": "d2e332",
                                    "bcl:num": "ii",
                                    "bcl:text": "execute any record required to give effect to that transfer, charge, encumbrance or other dealing, and"
                                }
                            ]
                        },
                        {
                            "@id": "d2e341",
                            "bcl:num": "b",
                            "bcl:text": [
                                "an official",
                                "must give the record the same effect as if it had been duly executed by the corporation."
                            ],
                            "bcl:subparagraph": [
                                {
                                    "@id": "d2e350",
                                    "bcl:num": "i",
                                    "bcl:text": "who has authority over a registry or other office, including, without limitation, the personal property registry and a land title office, in which title to or interests in the right, property or asset is registered or recorded, and"
                                },
                                {
                                    "@id": "d2e359",
                                    "bcl:num": "ii",
                                    "bcl:text": "to whom a record referred to in paragraph (a) (ii) executed by or on behalf of the government is submitted in support of the transfer, charge, encumbrance or other dealing"
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e373",
                    "bcl:marginalnote": {
                        "in:doc": "Land Act",
                        "#text": "Application of the"
                    },
                    "bcl:num": "6",
                    "bcl:subsection": [
                        {
                            "@id": "d2e383",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96245_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Act"
                                },
                                "#text": "Subject to subsections (2) and (3), the  does not apply to the following while under the minister's administration:"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e395",
                                    "bcl:num": "a",
                                    "bcl:text": "land referred to in section 2 (1);"
                                },
                                {
                                    "@id": "d2e404",
                                    "bcl:num": "b",
                                    "bcl:text": "land that is acquired by the minister under section 8 (1) (a);"
                                },
                                {
                                    "@id": "d2e414",
                                    "bcl:num": "c",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96245_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Land Act"
                                        },
                                        "#text": "land the administration of which is transferred to the minister under section 106 of the  or section 8 (3) of this Act;"
                                    }
                                },
                                {
                                    "@id": "d2e426",
                                    "bcl:num": "d",
                                    "bcl:text": {
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/00_14001_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "Provincial Capital Commission Dissolution Act"
                                        },
                                        "#text": "land transferred to and vested in the government under the ."
                                    }
                                }
                            ]
                        },
                        {
                            "@id": "d2e440",
                            "bcl:num": "2",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96245_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Act"
                                },
                                "#text": "Section 50 of the  applies to administered land."
                            }
                        },
                        {
                            "@id": "d2e452",
                            "bcl:num": "3",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96245_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Act"
                                },
                                "#text": "The Lieutenant Governor in Council may direct, by regulation, that one or more provisions of the , additional to the provision referred to in subsection (2), apply to some or all of the administered land."
                            }
                        },
                        {
                            "@id": "d2e464",
                            "bcl:num": "4",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96245_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Act"
                                },
                                "#text": "For the purposes of subsections (2) and (3), if a provision of the  applies to administered land, a reference in that provision to \"Crown land\" is deemed to be a reference to the administered land."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e477",
                    "bcl:marginalnote": "Transfer is not a default",
                    "bcl:num": "7",
                    "bcl:text": {
                        "bcl:link": {
                            "@resource": "leg",
                            "@xlink:href": "/legislation/96250_00",
                            "@xlink:type": "locator",
                            "in:doc": "Land Title Act"
                        },
                        "#text": "Despite any provision to the contrary in a record, including, without limitation, in a security agreement, commercial paper, lease, licence, permit or other contract, instrument, document or certificate, the transfer to the government of a right, property, asset, obligation or liability under sections 2 and 3 and an amendment to a register under section 383.1 of the  does not constitute a breach or contravention of, or an event of default under, the record, and, without limiting this, does not entitle any person who has an interest in the right, property, asset, obligation or liability to claim any damages, compensation or other remedy."
                    }
                },
                {
                    "@id": "d2e492",
                    "bcl:marginalnote": "Powers of minister",
                    "bcl:num": "8",
                    "bcl:subsection": [
                        {
                            "@id": "d2e500",
                            "bcl:num": "1",
                            "bcl:text": "The minister may do the following on any terms and conditions the minister considers appropriate:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e509",
                                    "bcl:num": "a",
                                    "bcl:text": "acquire land, on behalf of the government, by purchase, lease or otherwise, for present or future accommodation of a public agency or any undertaking in which a public agency is involved;"
                                },
                                {
                                    "@id": "d2e518",
                                    "bcl:num": "b",
                                    "bcl:text": "dispose of, or make available to any persons, all or any part of administered land;"
                                },
                                {
                                    "@id": "d2e528",
                                    "bcl:num": "c",
                                    "bcl:text": "provide to public agencies, in relation to land in which those public agencies have an interest or in which they, or an undertaking in which they are involved, are accommodated, one or more of services, accommodation and facilities, including, without limitation,",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e537",
                                            "bcl:num": "i",
                                            "bcl:text": "developing, constructing, altering, maintaining, managing and operating the land,"
                                        },
                                        {
                                            "@id": "d2e546",
                                            "bcl:num": "ii",
                                            "bcl:text": "acting as a general contractor or project manager, and"
                                        },
                                        {
                                            "@id": "d2e555",
                                            "bcl:num": "iii",
                                            "bcl:text": "acquiring, by purchase, rental or otherwise, equipment, fixtures and other property, real or personal and movable or immovable, required for the land;"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e564",
                                    "bcl:num": "d",
                                    "bcl:text": "set and levy a fee or charge to be paid by the person to whom a disposition is made under paragraph (b) or to whom a service, accommodation or facility is provided under paragraph (c)."
                                }
                            ]
                        },
                        {
                            "@id": "d2e573",
                            "bcl:num": "2",
                            "bcl:text": "The minister may transfer the administration of administered land to another minister of the government with the consent of that other minister."
                        },
                        {
                            "@id": "d0e632",
                            "bcl:num": "3",
                            "bcl:text": "With the consent of the minister, another minister of the government who has the administration of land may, for the purposes of this Act, transfer the administration of the land to the minister."
                        }
                    ]
                },
                {
                    "@id": "d2e583",
                    "bcl:marginalnote": "Corporation dissolved",
                    "bcl:num": "9",
                    "bcl:text": "The corporation is dissolved and disestablished and the appointment of each director of the corporation is rescinded."
                },
                {
                    "@id": "d2e595",
                    "bcl:marginalnote": "Power to make regulations",
                    "bcl:num": "10",
                    "bcl:subsection": [
                        {
                            "@id": "d2e603",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96238_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Interpretation Act"
                                },
                                "#text": "The Lieutenant Governor in Council may make regulations referred to in section 41 of the ."
                            }
                        },
                        {
                            "@id": "d2e615",
                            "bcl:num": "2",
                            "bcl:text": "Without limiting subsection (1), the Lieutenant Governor in Council may make regulations as follows:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e624",
                                    "bcl:num": "a",
                                    "bcl:text": "defining any word or expression used but not defined in this Act;"
                                },
                                {
                                    "@id": "d2e634",
                                    "bcl:num": "b",
                                    "bcl:text": "prescribing additional functions or activities to be carried out or engaged in by the minister;"
                                },
                                {
                                    "@id": "d2e643",
                                    "bcl:num": "c",
                                    "bcl:text": "respecting any other matter or thing that the Lieutenant Governor in Council considers necessary or advisable to carry out the intent of this Act."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e829",
                    "bcl:marginalnote": "Commencement",
                    "bcl:num": "21",
                    "bcl:text": "This Act comes into force by regulation of the Lieutenant Governor in Council."
                }
            ],
