{
    "act:act": {
        "@xmlns:act": "http://www.gov.bc.ca/2013/legislation/act",
        "@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
        "@xmlns:bcl": "http://www.gov.bc.ca/2013/bclegislation",
        "@xmlns:xlink": "http://www.w3.org/1999/xlink",
        "@xmlns:in": "http://www.qp.gov.bc.ca/2013/inline",
        "@xmlns:fun": "http://www.bclaws.ca/xslt/functions",
        "@xmlns:oasis": "http://docs.oasis-open.org/ns/oasis-exchange/table",
        "@xmlns:reg": "http://www.gov.bc.ca/2013/legislation/regulation",
        "@xsi:schemaLocation": "http://www.gov.bc.ca/2013/legislation/act http://standards.qp.gov.bc.ca/standards/2013/act_1.0.xsd",
        "@id": "00_08006_01",
        "act:title": "Musqueam Reconciliation, Settlement and Benefits Agreement Implementation\u00a0Act",
        "act:chapter": "6",
        "act:yearenacted": "2008",
        "act:assentedto": "March 31, 2008",
        "act:currency": {
            "act:tlcDirPath": "1325524918/08006/tlc08006_f",
            "act:tlc": "E3tlc08006"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e18",
                    "bcl:marginalnote": "Definitions",
                    "bcl:num": "1",
                    "bcl:text": "In this Act:",
                    "bcl:definition": [
                        {
                            "@id": "d2e29",
                            "bcl:text": {
                                "in:term": "agreement",
                                "#text": "means the Reconciliation, Settlement and Benefits Agreement between the Musqueam Indian Band and Her Majesty the Queen in right of the Province of British Columbia dated for reference March 11, 2008;"
                            }
                        },
                        {
                            "@id": "d2e38",
                            "bcl:text": {
                                "in:term": "Block F",
                                "#text": "means those lands legally described as PID 013-763-938, Block F, District Lot 140, Group 1, New Westminster District;"
                            }
                        },
                        {
                            "@id": "d2e48",
                            "bcl:text": {
                                "in:term": "Block K",
                                "#text": "means those lands legally described as PID 013-764-021, Block K, District Lot 140, Group 1, New Westminster District;"
                            }
                        },
                        {
                            "@id": "d2e57",
                            "bcl:text": {
                                "in:term": "charge",
                                "bcl:link": [
                                    {
                                        "@resource": "leg",
                                        "@xlink:href": "/legislation/96250_00",
                                        "@xlink:type": "locator",
                                        "in:doc": "Land Title Act"
                                    },
                                    {
                                        "@resource": "leg",
                                        "@xlink:href": "/legislation/96469_01",
                                        "@xlink:type": "locator",
                                        "in:doc": "University Endowment Land Act"
                                    }
                                ],
                                "#text": "has the same meaning as in the  and includes any notations against an indefeasible title and, in section 3 (1) (a), includes covenants described in section 17 of the ;"
                            }
                        },
                        {
                            "@id": "d2e72",
                            "bcl:text": {
                                "in:term": "designated company",
                                "#text": "means a corporation specified by regulation of the minister under section 11 for the purposes of sections 2 and 3 (1);"
                            }
                        },
                        {
                            "@id": "d2e81",
                            "bcl:text": {
                                "in:term": "effective date",
                                "#text": "means the date that is specified as the effective date by regulation of the minister under section 11;"
                            }
                        },
                        {
                            "@id": "d2e90",
                            "bcl:text": {
                                "in:term": "instrument",
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96250_00",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Title Act"
                                },
                                "#text": "has the same meaning as in the ;"
                            }
                        },
                        {
                            "@id": "d2e102",
                            "bcl:text": {
                                "in:term": "land title office",
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96250_00",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Title Act"
                                },
                                "#text": "has the same meaning as in the ;"
                            }
                        },
                        {
                            "@id": "d2e115",
                            "bcl:text": {
                                "in:term": "lease",
                                "#text": "means the lease in favour of UGCC Holdings Inc. dated May 17, 1985 and registered in the land title office under number N40074, as modified by those instruments registered in the land title office under numbers GB125274, GD98736 and BV92595;"
                            }
                        },
                        {
                            "@id": "d2e124",
                            "bcl:text": {
                                "in:term": "minister",
                                "#text": "means the Minister of Aboriginal Relations and Reconciliation;"
                            }
                        },
                        {
                            "@id": "d2e133",
                            "bcl:text": {
                                "in:term": "Musqueam Indian Band",
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "http://laws-lois.justice.gc.ca/eng/acts/i-5/",
                                    "@xlink:type": "locator",
                                    "in:doc": "Indian Act"
                                },
                                "#text": "means the band, as defined in the  (Canada), named the Musqueam Indian Band, and the Musqueam Nation or Musqueam First Nation, all of which must be interpreted to refer to the same collective group of traditional aboriginal peoples;"
                            }
                        },
                        {
                            "@id": "d2e145",
                            "bcl:text": {
                                "in:term": "registrar",
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96250_00",
                                    "@xlink:type": "locator",
                                    "in:doc": "Land Title Act"
                                },
                                "#text": "has the same meaning as in the ;"
                            }
                        },
                        {
                            "@id": "d2e157",
                            "bcl:text": {
                                "in:term": "UBC Golf Course lands",
                                "#text": "means those lands legally described as"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e166",
                                    "bcl:num": "a",
                                    "bcl:text": "PID 006-707-289, Block A, District Lot 3900, Group 1, NWD, Plan 20266, and"
                                },
                                {
                                    "@id": "d2e176",
                                    "bcl:num": "b",
                                    "bcl:text": "PID 006-707-483, Block B, District Lot 3900, Group 1, NWD, Plan 20266."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e186",
                    "bcl:marginalnote": "Vesting of UBC Golf Course Lands",
                    "bcl:num": "2",
                    "bcl:text": {
                        "bcl:link": {
                            "@resource": "leg",
                            "@xlink:href": "/legislation/96468_01",
                            "@xlink:type": "locator",
                            "in:doc": "University Act"
                        },
                        "#text": "On the effective date, and despite sections 27, 46.1, 50 (2) (a) and 53 of the  and any other enactment or law to the contrary,"
                    },
                    "bcl:paragraph": [
                        {
                            "@id": "d2e200",
                            "bcl:num": "a",
                            "bcl:text": "a fee simple estate in the UBC Golf Course lands is vested in the applicable designated company free and clear of all charges other than the following:",
                            "bcl:subparagraph": [
                                {
                                    "@id": "d2e209",
                                    "bcl:num": "i",
                                    "bcl:text": "the statutory right of way registered against the title to Block A in the land title office in favour of Her Majesty the Queen in right of the Province of British Columbia under number N31664;"
                                },
                                {
                                    "@id": "d2e219",
                                    "bcl:num": "ii",
                                    "bcl:text": "the lease;"
                                },
                                {
                                    "@id": "d2e228",
                                    "bcl:num": "iii",
                                    "bcl:text": "the mortgage of the lease registered against the title to Block B in the land title office in favour of the Bank of British Columbia under number P1480;"
                                },
                                {
                                    "@id": "d2e237",
                                    "bcl:num": "iv",
                                    "bcl:text": "the sub-lease of the lease registered against the title to Block A in the land title office in favour of The British Columbia Golf House Society under number GB143237;"
                                },
                                {
                                    "@id": "d2e246",
                                    "bcl:num": "v",
                                    "bcl:text": "the mortgage of the lease registered in the land title office in favour of Hongkong Bank of Canada under number GD44966, as modified by those instruments registered in the land title office under numbers BP77824 and BB253596;"
                                },
                                {
                                    "@id": "d2e255",
                                    "bcl:num": "vi",
                                    "bcl:text": "the undersurface rights and other reservations and exceptions registered in the land title office under number BW194454;"
                                },
                                {
                                    "@id": "d2e264",
                                    "bcl:num": "vii",
                                    "bcl:text": "any other charges added by regulation under section 11, and"
                                }
                            ]
                        },
                        {
                            "@id": "d2e274",
                            "bcl:num": "b",
                            "bcl:text": "the benefit of all covenants in the lease in favour of the lessor are vested in the applicable designated company."
                        }
                    ]
                },
                {
                    "@id": "d2e284",
                    "bcl:marginalnote": "Vesting of Block F and Block K",
                    "bcl:num": "3",
                    "bcl:subsection": [
                        {
                            "@id": "d2e292",
                            "bcl:num": "1",
                            "bcl:text": "On the effective date, and despite any other enactment or law to the contrary,",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e301",
                                    "bcl:num": "a",
                                    "bcl:text": "a fee simple estate in Block F is vested in the applicable designated company free and clear of all charges other than the following:",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e310",
                                            "bcl:num": "i",
                                            "bcl:text": {
                                                "bcl:link": {
                                                    "@resource": "leg",
                                                    "@xlink:href": "/legislation/96245_01",
                                                    "@xlink:type": "locator",
                                                    "in:doc": "Land Act"
                                                },
                                                "#text": "all exceptions and reservations set out in section 50 (1) of the , as if the vesting referred to in this paragraph were a disposition of Crown land under that Act;"
                                            }
                                        },
                                        {
                                            "@id": "d2e323",
                                            "bcl:num": "ii",
                                            "bcl:text": "the statutory right of way registered in the land title office in favour of Her Majesty the Queen in right of the Province of British Columbia under number GC51902;"
                                        },
                                        {
                                            "@id": "d2e332",
                                            "bcl:num": "iii",
                                            "bcl:text": "the statutory right of way registered in the land title office in favour of BC Gas Inc. under number BE313681;"
                                        },
                                        {
                                            "@id": "d2e341",
                                            "bcl:num": "iv",
                                            "bcl:text": {
                                                "bcl:link": {
                                                    "@resource": "leg",
                                                    "@xlink:href": "/legislation/00_96483_01a",
                                                    "@xlink:type": "locator",
                                                    "in:doc": "Water Act"
                                                },
                                                "#text": "any conditional or final water licence or substituted water licence issued or given under the , or any prior enactment of the Province of British Columbia of like effect, and the rights of the holder of it as of April 21, 1989, to enter on the land and to maintain, repair and operate any works permitted on the land under the licence;"
                                            }
                                        },
                                        {
                                            "@id": "d2e353",
                                            "bcl:num": "v",
                                            "bcl:text": "any other charges added by regulation under section 11, and"
                                        }
                                    ]
                                },
                                {
                                    "@id": "d2e362",
                                    "bcl:num": "b",
                                    "bcl:text": "a fee simple estate in Block K is vested in the applicable designated company free and clear of all charges other than the following:",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e371",
                                            "bcl:num": "i",
                                            "bcl:text": {
                                                "bcl:link": {
                                                    "@resource": "leg",
                                                    "@xlink:href": "/legislation/96245_01",
                                                    "@xlink:type": "locator",
                                                    "in:doc": "Land Act"
                                                },
                                                "#text": "all exceptions and reservations set out in section 50 (1) of the , as if the vesting referred to in this paragraph were a disposition of Crown land under that Act;"
                                            }
                                        },
                                        {
                                            "@id": "d2e384",
                                            "bcl:num": "ii",
                                            "bcl:text": "the statutory right of way registered in the land title office in favour of Her Majesty the Queen in right of the Province of British Columbia under number GC51902;"
                                        },
                                        {
                                            "@id": "d2e393",
                                            "bcl:num": "iii",
                                            "bcl:text": {
                                                "bcl:link": {
                                                    "@resource": "leg",
                                                    "@xlink:href": "/legislation/00_96483_01a",
                                                    "@xlink:type": "locator",
                                                    "in:doc": "Water Act"
                                                },
                                                "#text": "any conditional or final water licence or substituted water licence issued or given under the , or any prior enactment of the Province of British Columbia of like effect, and the rights of the holder of it as of April 21, 1989, to enter on the land and to maintain, repair and operate any works permitted on the land under the licence;"
                                            }
                                        },
                                        {
                                            "@id": "d2e405",
                                            "bcl:num": "iv",
                                            "bcl:text": "any other charges added by regulation under section 11."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e414",
                            "bcl:num": "2",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/r15001_00",
                                    "@xlink:type": "locator",
                                    "in:doc": "Local Government Act"
                                },
                                "#text": "On the effective date, Block F and Block K cease to be a regional park within the meaning of the , and, without limitation, the vestings referred to in this section take effect free of any dedication to the public."
                            }
                        },
                        {
                            "@id": "d2e426",
                            "bcl:num": "3",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96469_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "University Endowment Land Act"
                                },
                                "#text": "On the effective date, and despite the , the minister responsible for the administration of that Act may, on behalf of the government, indicate in writing the minister's support for the addition of Block\u00a0K to Musqueam Indian Reserve No.\u00a02."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e439",
                    "bcl:marginalnote": "Land Use, Building and Community Administration Bylaw and the Official Community Plan",
                    "bcl:num": "4",
                    "bcl:subsection": [
                        {
                            "@id": "d2e447",
                            "bcl:num": "1",
                            "bcl:text": "In this section:",
                            "bcl:definition": [
                                {
                                    "@id": "d2e456",
                                    "bcl:text": {
                                        "in:term": "Land Use, Building and Community Administration Bylaw",
                                        "in:doc": "University Endowment Land Amendment Act, 1989",
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96469_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "University Endowment Land Act"
                                        },
                                        "#text": "means the Land Use, Building and Community Administration Bylaw as confirmed and validated by section 4 of the , S.B.C. 1989, c. 41, and amended under the ;"
                                    }
                                },
                                {
                                    "@id": "d2e471",
                                    "bcl:text": {
                                        "in:term": "Official Community Plan",
                                        "bcl:link": {
                                            "@resource": "leg",
                                            "@xlink:href": "/legislation/96469_01",
                                            "@xlink:type": "locator",
                                            "in:doc": "University Endowment Land Act"
                                        },
                                        "#text": "means the University Endowment Lands Official Community Plan, adopted on October 4, 2005 by the minister responsible for the administration of the ."
                                    }
                                }
                            ]
                        },
                        {
                            "@id": "d2e484",
                            "bcl:num": "2",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96469_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "University Endowment Land Act"
                                },
                                "#text": "On the effective date, despite the  and section 8 of the Land Use, Building and Community Administration Bylaw and subject to subsection (3) of this section,"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e496",
                                    "bcl:num": "a",
                                    "bcl:text": "Schedule 1 to the Land Use, Building and Community Administration Bylaw is conclusively deemed to be amended to include Block F in the Multiple Dwelling District, Low-Rise (MF-1) Land Use District, and"
                                },
                                {
                                    "@id": "d2e505",
                                    "bcl:num": "b",
                                    "bcl:text": "the Official Community Plan is conclusively deemed to be amended in",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e514",
                                            "bcl:num": "i",
                                            "bcl:text": "section 2.2 to include Block F in the description of Area D,"
                                        },
                                        {
                                            "@id": "d2e523",
                                            "bcl:num": "ii",
                                            "bcl:text": "Figure 1 to include Block F in Area D,"
                                        },
                                        {
                                            "@id": "d2e532",
                                            "bcl:num": "iii",
                                            "bcl:text": "Figure 2 to include Block F in RMF1: MULTI-FAMILY (low rise), and"
                                        },
                                        {
                                            "@id": "d2e542",
                                            "bcl:num": "iv",
                                            "bcl:text": "Figure 3 to exclude Block F from Pacific Spirit Regional Park."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e551",
                            "bcl:num": "3",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96469_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "University Endowment Land Act"
                                },
                                "#text": "Subsection (2) does not operate and must not be interpreted to prevent the enactment or variation of a bylaw under the authority of the  after the effective date, including, without limitation, a bylaw or variation of a bylaw that applies to Block F."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e564",
                    "bcl:marginalnote": "Minister's direction to the registrar",
                    "bcl:num": "5",
                    "bcl:subsection": [
                        {
                            "@id": "d2e572",
                            "bcl:num": "1",
                            "bcl:text": "On making a regulation under section 11 (1) (a), (b) or (c), the minister may issue and file with the registrar a certificate that certifies the following:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e581",
                                    "bcl:num": "a",
                                    "bcl:text": "the legal descriptions of the UBC Golf Course lands, Block F and Block K;"
                                },
                                {
                                    "@id": "d2e590",
                                    "bcl:num": "b",
                                    "bcl:text": "in accordance with section 11, the name of the designated company in which the fee simple estate to each of the UBC Golf Course lands, Block F and Block K is vested;"
                                },
                                {
                                    "@id": "d2e600",
                                    "bcl:num": "c",
                                    "bcl:text": "the charges set out in sections 2 (a) and 3 (1) (a) and (b) against the title to each of the UBC Golf Course lands, Block F and Block K."
                                }
                            ]
                        },
                        {
                            "@id": "d2e609",
                            "bcl:num": "2",
                            "bcl:text": "The certificate referred to in subsection (1) constitutes conclusive evidence to the registrar that the designated company named in the certificate is entitled to a good safe holding and marketable title in fee simple in respect of the UBC Golf Course lands, Block F or Block K, as the case may be, subject only to the charges set out in the certificate."
                        },
                        {
                            "@id": "d2e618",
                            "bcl:num": "3",
                            "bcl:text": "On receiving a certificate of the minister under subsection (1), the registrar must",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e627",
                                    "bcl:num": "a",
                                    "bcl:text": "cancel any charges that are not set out in the certificate and any instruments registrable in the same manner as a charge registered against the title to the UBC Golf Course lands, Block F or Block K before cancellation of the indefeasible titles under paragraph (c),"
                                },
                                {
                                    "@id": "d2e636",
                                    "bcl:num": "b",
                                    "bcl:text": "register an indefeasible title to each of the UBC Golf Course lands, Block F and Block K in the name of the designated company subject to the charges referred to in subsection (1) (c), and"
                                },
                                {
                                    "@id": "d2e645",
                                    "bcl:num": "c",
                                    "bcl:text": "cancel the existing indefeasible titles to the UBC Golf Course lands, Block F and Block K."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e656",
                    "bcl:marginalnote": {
                        "in:doc": "Land Title Act",
                        "#text": "Section 23 (2) of the"
                    },
                    "bcl:num": "6",
                    "bcl:text": {
                        "bcl:link": {
                            "@resource": "leg",
                            "@xlink:href": "/legislation/96250_00",
                            "@xlink:type": "locator",
                            "in:doc": "Land Title Act"
                        },
                        "#text": "Section 23 (2) (a) of the  does not apply to the indefeasible titles to Block F or Block K registered in accordance with section 5 (3) (b) or any indefeasible title derived from them and the registrar must endorse each indefeasible title to which this section applies with a notation to this effect."
                    }
                },
                {
                    "@id": "d2e673",
                    "bcl:marginalnote": "Compensation",
                    "bcl:num": "7",
                    "bcl:subsection": [
                        {
                            "@id": "d2e681",
                            "bcl:num": "1",
                            "bcl:text": "On the effective date, the amount of $20.8 million is payable to the University of British Columbia by the government as compensation for the vesting referred to in section 2."
                        },
                        {
                            "@id": "d2e690",
                            "bcl:num": "2",
                            "bcl:text": "The compensation payable under subsection (1) constitutes full and final settlement of any and all claims by the University of British Columbia in relation to the vesting referred to in section 2."
                        },
                        {
                            "@id": "d2e699",
                            "bcl:num": "3",
                            "bcl:text": "No compensation is payable to the Greater Vancouver Regional District as a consequence of the vestings referred to in section 3 and any and all claims by the Greater Vancouver Regional District in relation to the vestings referred to in section 3 are extinguished."
                        }
                    ]
                },
                {
                    "@id": "d2e709",
                    "bcl:marginalnote": "No legal proceeding",
                    "bcl:num": "8",
                    "bcl:text": "No legal proceeding for damages or compensation lies or may be commenced or maintained against the government, the Musqueam Indian Band or a designated company in respect of a matter referred to in section 2, 3 or 4."
                },
                {
                    "@id": "d2e721",
                    "bcl:marginalnote": "Appropriation",
                    "bcl:num": "9",
                    "bcl:subsection": [
                        {
                            "@id": "d2e729",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96138_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Financial Administration Act"
                                },
                                "#text": "Despite section 21 (3) of the , the minister may pay out of the consolidated revenue fund amounts required for the purposes of"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e741",
                                    "bcl:num": "a",
                                    "bcl:text": "implementing the agreement and this Act, and"
                                },
                                {
                                    "@id": "d2e750",
                                    "bcl:num": "b",
                                    "bcl:text": "fulfilling the obligations of the government under the agreement and this Act."
                                }
                            ]
                        },
                        {
                            "@id": "d2e760",
                            "bcl:num": "2",
                            "bcl:text": [
                                "For the purposes of subsection (1),",
                                "are conclusively deemed in relation to the government to have occurred or been made on the earlier of the date of the occurrence or payment and March 31, 2008."
                            ],
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e769",
                                    "bcl:num": "a",
                                    "bcl:text": "a circumstance, a transaction, a matter or an event that occurs and gives rise to a payment under that subsection, and"
                                },
                                {
                                    "@id": "d2e778",
                                    "bcl:num": "b",
                                    "bcl:text": "a payment made under that subsection"
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e791",
                    "bcl:marginalnote": {
                        "in:doc": "Balanced Budget and Ministerial Accountability Act"
                    },
                    "bcl:num": "10",
                    "bcl:text": {
                        "bcl:link": {
                            "@resource": "leg",
                            "@xlink:href": "/legislation/01028_01",
                            "@xlink:type": "locator",
                            "in:doc": "Balanced Budget and Ministerial Accountability Act"
                        },
                        "#text": "Amounts of money required to be provided or paid under section 9 (1) are not to be taken into account for the purposes of calculating the actual amounts of operating expenses under section 5 (1) of the ."
                    }
                },
                {
                    "@id": "d2e809",
                    "bcl:marginalnote": "Power to make regulations",
                    "bcl:num": "11",
                    "bcl:subsection": [
                        {
                            "@id": "d2e817",
                            "bcl:num": "1",
                            "bcl:text": "The minister may by regulation",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e826",
                                    "bcl:num": "a",
                                    "bcl:text": "specify a date as the effective date for the purposes of this Act,"
                                },
                                {
                                    "@id": "d2e842",
                                    "bcl:num": "b",
                                    "bcl:text": "designate a corporation for the purposes of sections 2 and 3 (1), and"
                                },
                                {
                                    "@id": "d2e851",
                                    "bcl:num": "c",
                                    "bcl:text": "[Repealed 2008-6-11 (3).]"
                                },
                                {
                                    "@id": "d2e860",
                                    "bcl:num": "d",
                                    "bcl:text": "correct or update a reference to",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e869",
                                            "bcl:num": "i",
                                            "bcl:text": "one or more charges referred to in section 2 (a) or 3 (1) (a) or (b), or"
                                        },
                                        {
                                            "@id": "d2e878",
                                            "bcl:num": "ii",
                                            "bcl:text": "one or more legal descriptions referred to in this Act."
                                        }
                                    ]
                                }
                            ],
                            "bcl:editorialnote": {
                                "in:em": "[Note: B.C. Reg. 71/2008 specified April 14, 2008 as the effective date for the purposes of this Act.]"
                            }
                        },
                        {
                            "@id": "d2e887",
                            "bcl:num": "2",
                            "bcl:text": "Under subsection (1) (b), the minister may designate one or more corporations and specify the purposes for which each corporation is designated."
                        },
                        {
                            "@id": "d2e897",
                            "bcl:num": "3",
                            "bcl:text": "Subsection (1) (c) is repealed on the day after the effective date."
                        },
                        {
                            "@id": "d2e906",
                            "bcl:num": "4",
                            "bcl:text": "The minister may not make a regulation under subsection (1) (d) that comes into force later than 30 days after the effective date."
                        }
                    ]
                },
                {
                    "@id": "d2e917",
                    "bcl:marginalnote": "This Act prevails",
                    "bcl:num": "12",
                    "bcl:text": "In the event of a conflict or an inconsistency between this Act or an enactment made under this Act and any other enactment, this Act prevails."
                },
                {
                    "@id": "d2e983",
                    "bcl:marginalnote": "Commencement",
                    "bcl:num": "14",
                    "bcl:text": "This Act comes into force on the date of Royal Assent."
                }
            ],
