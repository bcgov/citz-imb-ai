        "@id": "00_96426_01",
        "act:title": "Skagit Environmental Enhancement Act",
        "act:chapter": "426",
        "act:yearenacted": "1996",
        "act:currency": {
            "act:tlcDirPath": "1527898742/96426/tlc96426_f",
            "act:tlc": "tlc96426"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e15",
                    "bcl:marginalnote": "Definitions",
                    "bcl:num": "1",
                    "bcl:text": "In this Act:",
                    "bcl:definition": [
                        {
                            "@id": "d2e26",
                            "bcl:text": {
                                "in:term": "agreement",
                                "#text": "means the agreement dated March\u00a030, 1984 between the City of Seattle in the State of Washington, United States of America and the government;"
                            }
                        },
                        {
                            "@id": "d2e35",
                            "bcl:text": {
                                "in:term": "commission",
                                "#text": "means the Commission established under Appendix\u00a0D to the agreement;"
                            }
                        },
                        {
                            "@id": "d2e45",
                            "bcl:text": {
                                "in:term": "fund",
                                "#text": "means the Skagit Environmental Endowment Fund established under Appendix\u00a0D to the agreement."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e55",
                    "bcl:marginalnote": "Powers of commission",
                    "bcl:num": "2",
                    "bcl:text": "The commission has the powers and rights of a corporation in British Columbia including the right to acquire and dispose of real property."
                },
                {
                    "@id": "d2e67",
                    "bcl:marginalnote": "No taxation",
                    "bcl:num": "3",
                    "bcl:text": "The commission and the fund are not liable to taxation."
                },
                {
                    "@id": "d2e79",
                    "bcl:marginalnote": "Not government agent",
                    "bcl:num": "4",
                    "bcl:text": "The commission is not an agent of the government."
                },
                {
                    "@id": "d2e91",
                    "bcl:marginalnote": "Acts not applicable",
                    "bcl:num": "5",
                    "bcl:subsection": [
                        {
                            "@id": "d2e99",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96238_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Interpretation Act"
                                },
                                "#text": "Sections 17 (2) and 18 of the  do not apply to the commission."
                            }
                        },
                        {
                            "@id": "d2e111",
                            "bcl:num": "2",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/02057_00",
                                    "@xlink:type": "locator",
                                    "in:doc": "Business Corporations Act"
                                },
                                "#text": "The  does not apply to the commission, but the Lieutenant Governor in Council may order that one or more of the provisions apply."
                            }
                        }
                    ]
                }
            ]
        }
    }
