        "act:title": "Family Day Act",
        "act:chapter": "24",
        "act:yearenacted": "2012",
        "act:assentedto": "May 31, 2012",
        "act:currency": {
            "act:tlcDirPath": "1198514681/950549314/1238343794",
            "act:tlc": "E2tlc12024"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e24",
                    "bcl:marginalnote": "Family Day established as public holiday",
                    "bcl:num": "1",
                    "bcl:text": "The day in February prescribed under this Act is a public holiday to be observed as\n        \"Family Day\"."
                },
                {
                    "@id": "d2e36",
                    "bcl:marginalnote": "Day set by regulation",
                    "bcl:num": "2",
                    "bcl:text": "The Lieutenant Governor in Council may by regulation prescribe a day in February to\n        be observed as \"Family Day\"."
                },
                {
                    "@id": "d2e115",
                    "bcl:marginalnote": "Commencement",
                    "bcl:num": "5",
                    "bcl:text": "This Act comes into force on the date of Royal Assent."
                }
            ],
            "bcl:conseqhead": {
                "bcl:text": "Consequential Amendments",
                "bcl:num": "3-4",
                "bcl:conseqnote": {
