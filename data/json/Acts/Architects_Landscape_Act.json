        "@xsl-fo": "https://styles.qp.gov.bc.ca/bclaws-xsl-fo/act.xsl",
        "@xsi:schemaLocation": "http://www.gov.bc.ca/2013/legislation/act http://standards.qp.gov.bc.ca/standards/2013/act_1.0.xsd",
        "@id": "00_96018_01",
        "act:title": "Architects (Landscape) Act",
        "act:chapter": "18",
        "act:yearenacted": "1996",
        "act:currency": {
            "act:tlcDirPath": "524872423/96018/tlc96018_f",
            "act:tlc": "E3tlc96018"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e15",
                    "bcl:marginalnote": "Definitions",
                    "bcl:num": "1",
                    "bcl:text": "In this Act:",
                    "bcl:definition": [
                        {
                            "@id": "d2e26",
                            "bcl:text": {
                                "in:term": "board",
                                "#text": "means the board of directors of the society;"
                            }
                        },
                        {
                            "@id": "d2e35",
                            "bcl:text": {
                                "in:term": "society",
                                "#text": "means The British Columbia Society of Landscape Architects."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e45",
                    "bcl:marginalnote": "Society continued",
                    "bcl:num": "2",
                    "bcl:subsection": [
                        {
                            "@id": "d2e53",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/96433_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Society Act"
                                },
                                "#text": "The British Columbia Society of Landscape Architects, incorporated under the , and its members, are continued as a corporation under that name."
                            }
                        },
                        {
                            "@id": "d2e65",
                            "bcl:num": "2",
                            "bcl:text": "The society must have a common seal."
                        },
                        {
                            "@id": "d2e74",
                            "bcl:num": "3",
                            "bcl:text": "The society",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e84",
                                    "bcl:num": "a",
                                    "bcl:text": "has perpetual succession,"
                                },
                                {
                                    "@id": "d2e93",
                                    "bcl:num": "b",
                                    "bcl:text": "has power to acquire property,"
                                },
                                {
                                    "@id": "d2e102",
                                    "bcl:num": "c",
                                    "bcl:text": "may sell, exchange, mortgage, lease, let, improve and develop property, and"
                                },
                                {
                                    "@id": "d2e111",
                                    "bcl:num": "d",
                                    "bcl:text": "may erect and maintain buildings on its property."
                                }
                            ]
                        },
                        {
                            "@id": "d2e120",
                            "bcl:num": "4",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/15018_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Societies Act"
                                },
                                "#text": "The , other than the following provisions, applies to the society:"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d1e142",
                                    "bcl:num": "a",
                                    "bcl:text": {
                                        "in:desc": "[registrar may order society to change its name]",
                                        "#text": "section 9 (5) ;"
                                    }
                                },
                                {
                                    "@id": "d1e154",
                                    "bcl:num": "b",
                                    "bcl:text": {
                                        "in:desc": "[constitution]",
                                        "#text": "section 10 ;"
                                    }
                                },
                                {
                                    "@id": "d1e166",
                                    "bcl:num": "c",
                                    "bcl:text": {
                                        "in:desc": "[bylaws]",
                                        "#text": "section 11 ;"
                                    }
                                },
                                {
                                    "@id": "d1e179",
                                    "bcl:num": "d",
                                    "bcl:text": {
                                        "in:desc": [
                                            "[Alterations to Constitution and Bylaws]",
                                            "[Fundamental Matters in Relation to Societies]"
                                        ],
                                        "#text": "Division 4  of Part 2 ;"
                                    }
                                },
                                {
                                    "@id": "d1e194",
                                    "bcl:num": "e",
                                    "bcl:text": {
                                        "in:desc": [
                                            "[Amalgamation]",
                                            "[Corporate Reorganizations]"
                                        ],
                                        "#text": "Division 1  of Part 7 ;"
                                    }
                                },
                                {
                                    "@id": "d1e209",
                                    "bcl:num": "f",
                                    "bcl:text": {
                                        "in:desc": "[dissolution by request of society without assets or liabilities]",
                                        "#text": "section 126 ;"
                                    }
                                },
                                {
                                    "@id": "d1e221",
                                    "bcl:num": "g",
                                    "bcl:text": {
                                        "in:desc": "[Extraprovincial Non-Share Corporations]",
                                        "#text": "Part 11 ;"
                                    }
                                },
                                {
                                    "@id": "d1e233",
                                    "bcl:num": "h",
                                    "bcl:text": {
                                        "in:desc": "[investigation of society]",
                                        "#text": "section 213 ;"
                                    }
                                },
                                {
                                    "@id": "d1e245",
                                    "bcl:num": "i",
                                    "bcl:text": {
                                        "in:desc": "[involuntary dissolution by registrar]",
                                        "#text": "section 214 ;"
                                    }
                                },
                                {
                                    "@id": "d1e258",
                                    "bcl:num": "j",
                                    "bcl:text": {
                                        "in:desc": "[dissolution by Lieutenant Governor in Council]",
                                        "#text": "section 215 ."
                                    }
                                }
                            ]
                        },
                        {
                            "@id": "d2e135",
                            "bcl:num": "5",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/15018_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Societies Act",
                                    "#text": ","
                                },
                                "#text": "If there is a conflict or inconsistency between this Act and the  this Act prevails."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e148",
                    "bcl:marginalnote": "Suits and contracts",
                    "bcl:num": "3",
                    "bcl:text": "The society may sue and be sued, and may contract and be contracted with, in its corporate name."
                },
                {
                    "@id": "d2e160",
                    "bcl:marginalnote": "Financial powers",
                    "bcl:num": "4",
                    "bcl:text": "The society may, subject to any conditions required by its bylaws,",
                    "bcl:paragraph": [
                        {
                            "@id": "d2e171",
                            "bcl:num": "a",
                            "bcl:text": "borrow, raise or secure the payment of money, and"
                        },
                        {
                            "@id": "d2e180",
                            "bcl:num": "b",
                            "bcl:text": "draw, make, accept, endorse, discount, execute and issue promissory notes, bills of exchange, and other negotiable or transferable instruments."
                        }
                    ]
                },
                {
                    "@id": "d2e190",
                    "bcl:marginalnote": "Head office",
                    "bcl:num": "5",
                    "bcl:subsection": [
                        {
                            "@id": "d2e198",
                            "bcl:num": "1",
                            "bcl:text": "The head office of the society must be in the City of Vancouver."
                        },
                        {
                            "@id": "d2e207",
                            "bcl:num": "2) to (6",
                            "bcl:text": "[Repealed 2015-18-255.]"
                        }
                    ]
                },
                {
                    "@id": "d2e273",
                    "bcl:marginalnote": "Membership",
                    "bcl:num": "6",
                    "bcl:subsection": [
                        {
                            "@id": "d2e281",
                            "bcl:num": "1",
                            "bcl:text": "On payment of the specified fee, if any, and written application, the board must register as a member of the society a person who satisfies the board that the person has passed the examinations set by the board of examiners."
                        },
                        {
                            "@id": "d2e290",
                            "bcl:num": "2",
                            "bcl:text": "On payment of the specified fee, if any, and written application, the board may accept as a member of the society a person who satisfies the board that the person",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e299",
                                    "bcl:num": "a",
                                    "bcl:text": "has passed examinations outside British Columbia equivalent to those set by the board of examiners for candidates in British Columbia, or"
                                },
                                {
                                    "@id": "d2e309",
                                    "bcl:num": "b",
                                    "bcl:text": "is or has been practising landscape architecture outside British Columbia and is a member in good standing of a similar society or other entity or group in the jurisdiction in which the person is or has been practising, the membership qualifications for which are at least equivalent to the qualifications required for candidates for membership within British Columbia."
                                }
                            ]
                        },
                        {
                            "@id": "d2e318",
                            "bcl:num": "3",
                            "bcl:text": "The board of directors may also register persons as junior members, student members, honorary members and associate members, in accordance with and subject to the bylaws of the society."
                        }
                    ]
                },
                {
                    "@id": "d2e328",
                    "bcl:marginalnote": "Board of directors",
                    "bcl:num": "7",
                    "bcl:subsection": [
                        {
                            "@id": "d2e336",
                            "bcl:num": "1",
                            "bcl:text": "The affairs of the society must be managed by the board."
                        },
                        {
                            "@id": "d2e345",
                            "bcl:num": "2",
                            "bcl:text": "The board must be elected and appointed and holds office in accordance with the bylaws of the society."
                        }
                    ]
                },
                {
                    "@id": "d2e355",
                    "bcl:marginalnote": "Board of examiners",
                    "bcl:num": "8",
                    "bcl:subsection": [
                        {
                            "@id": "d2e363",
                            "bcl:num": "1",
                            "bcl:text": "The board of examiners consists of the following:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e372",
                                    "bcl:num": "a",
                                    "bcl:text": "the president or a director of the society designated for the purpose by the president of the society;"
                                },
                                {
                                    "@id": "d2e381",
                                    "bcl:num": "b",
                                    "bcl:text": "the past president or a director of the society designated for the purpose by the president of the society;"
                                },
                                {
                                    "@id": "d2e391",
                                    "bcl:num": "c",
                                    "bcl:text": "4 or more persons appointed by the Lieutenant Governor in Council including",
                                    "bcl:subparagraph": [
                                        {
                                            "@id": "d2e400",
                                            "bcl:num": "i",
                                            "bcl:text": "a person nominated by the President of Kwantlen Polytechnic University,"
                                        },
                                        {
                                            "@id": "d2e409",
                                            "bcl:num": "ii",
                                            "bcl:text": "a person nominated by the Director of the School of Architecture and Landscape Architecture of The University of British Columbia,"
                                        },
                                        {
                                            "@id": "d2e418",
                                            "bcl:num": "iii",
                                            "bcl:text": "the head of the Landscape Architecture program of The University of British Columbia or a person nominated by the Dean of the Faculty of Applied Science of The University of British Columbia, and"
                                        },
                                        {
                                            "@id": "d2e427",
                                            "bcl:num": "iv",
                                            "bcl:text": "a person chosen to represent the public interest in the development and maintenance of proper standards of professional practice in landscape architecture in British Columbia."
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "@id": "d2e436",
                            "bcl:num": "2",
                            "bcl:text": "The board of examiners must set and hold regular examinations in British Columbia at least once in each year at the place and the time specified by the board of examiners."
                        },
                        {
                            "@id": "d2e446",
                            "bcl:num": "3",
                            "bcl:text": "The board of examiners may set and hold special examinations on the terms and conditions regarding the defraying of the expenses of the examinations as the board of examiners may specify in each instance."
                        },
                        {
                            "@id": "d2e455",
                            "bcl:num": "4",
                            "bcl:text": "The board of examiners must prescribe the rules of procedure to be followed and enforced at all examinations."
                        },
                        {
                            "@id": "d2e464",
                            "bcl:num": "5",
                            "bcl:text": "The board of examiners may appoint one or more of its members to preside at any regular or special examination or part of an examination."
                        },
                        {
                            "@id": "d2e473",
                            "bcl:num": "6",
                            "bcl:text": "As soon as feasible after holding an examination, the board of examiners must notify the board of the results."
                        }
                    ]
                },
                {
                    "@id": "d2e483",
                    "bcl:marginalnote": "Bylaws continue",
                    "bcl:num": "9",
                    "bcl:subsection": [
                        {
                            "@id": "d2e491",
                            "bcl:num": "1",
                            "bcl:text": "So far as not inconsistent with this Act, the bylaws of The British Columbia Society of Landscape Architects are the bylaws of the society."
                        },
                        {
                            "@id": "d2e500",
                            "bcl:num": "2",
                            "bcl:text": "The bylaws of the society must not be altered or added to except by an extraordinary resolution of the society."
                        },
                        {
                            "@id": "d2e509",
                            "bcl:num": "3",
                            "bcl:text": "An extraordinary resolution of the society altering or adding to the bylaws must be filed in duplicate with the Registrar of Companies, who must register one copy and return the other copy, certified as having been registered by the registrar."
                        },
                        {
                            "@id": "d2e519",
                            "bcl:num": "4",
                            "bcl:text": "Nothing that is in conflict with this Act may be included in the bylaws, and the bylaws must not contain anything contrary to law."
                        },
                        {
                            "@id": "d2e528",
                            "bcl:num": "5",
                            "bcl:text": "The bylaws of the society and any amendments to them must be published in one issue of the Gazette."
                        }
                    ]
                },
                {
                    "@id": "d2e538",
                    "bcl:marginalnote": "Bylaws",
                    "bcl:num": "10",
                    "bcl:subsection": [
                        {
                            "@id": "d2e546",
                            "bcl:num": "1",
                            "bcl:text": "Subject to section 9 (2), the board may make bylaws for all purposes relating to the affairs, business, property and objects of the society."
                        },
                        {
                            "@id": "d2e555",
                            "bcl:num": "2",
                            "bcl:text": "Without limiting subsection (1), the power of the board to make bylaws includes all of the following:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e564",
                                    "bcl:num": "a",
                                    "bcl:text": "registration of members, issue of membership certificates, and qualifications for registration of junior members, student members, honorary members and associate members;"
                                },
                                {
                                    "@id": "d2e574",
                                    "bcl:num": "b",
                                    "bcl:text": "holding, place and conduct of annual meetings and other meetings of the society, notices and consents prerequisite to the meetings, quorums, voting rights at meetings, and all other matters connected with the meetings;"
                                },
                                {
                                    "@id": "d2e583",
                                    "bcl:num": "c",
                                    "bcl:text": "qualifications of directors, their numbers, term of office, and all matters relating to their election and appointment;"
                                },
                                {
                                    "@id": "d2e592",
                                    "bcl:num": "d",
                                    "bcl:text": "filling of vacancies in the board;"
                                },
                                {
                                    "@id": "d2e601",
                                    "bcl:num": "e",
                                    "bcl:text": "matters relating to meetings of the board;"
                                },
                                {
                                    "@id": "d2e610",
                                    "bcl:num": "f",
                                    "bcl:text": "appointment, functions, duties and removal of officers and servants of the society and their remuneration, if any;"
                                },
                                {
                                    "@id": "d2e619",
                                    "bcl:num": "g",
                                    "bcl:text": "establishment of executive, membership and other committees of the society, membership in them, and their powers and duties;"
                                },
                                {
                                    "@id": "d2e629",
                                    "bcl:num": "h",
                                    "bcl:text": "keeping of records and making of reports for and by the society;"
                                },
                                {
                                    "@id": "d2e638",
                                    "bcl:num": "i",
                                    "bcl:text": "making of contracts for and by the society and the signing and issuance of cheques, drafts or other orders for payment of money, notes or other evidences of indebtedness by the society;"
                                },
                                {
                                    "@id": "d2e647",
                                    "bcl:num": "j",
                                    "bcl:text": "custody and use of the society seal;"
                                },
                                {
                                    "@id": "d2e656",
                                    "bcl:num": "k",
                                    "bcl:text": "establishing, levying, paying, remitting and collecting of membership and registration fees, their amounts, and requirements regarding payment;"
                                },
                                {
                                    "@id": "d2e665",
                                    "bcl:num": "l",
                                    "bcl:text": "conduct generally of the affairs of the society."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e675",
                    "bcl:marginalnote": "Objects",
                    "bcl:num": "11",
                    "bcl:text": "The objects of the society are the following:",
                    "bcl:paragraph": [
                        {
                            "@id": "d2e686",
                            "bcl:num": "a",
                            "bcl:text": "to uphold public health, safety and welfare as it relates to the professional practice of landscape architecture in British Columbia;"
                        },
                        {
                            "@id": "d2e695",
                            "bcl:num": "b",
                            "bcl:text": "to nurture and further the professional application of landscape architectural knowledge and technique as it relates to the planning, design, development, preservation, protection, restoration, reclamation, rehabilitation, enhancement and management of the environment;"
                        },
                        {
                            "@id": "d2e705",
                            "bcl:num": "c",
                            "bcl:text": "to advance landscape architectural knowledge and technique;"
                        },
                        {
                            "@id": "d2e714",
                            "bcl:num": "d",
                            "bcl:text": "to further and maintain proper standards of professional landscape architectural practice in British Columbia."
                        }
                    ]
                },
                {
                    "@id": "d2e725",
                    "bcl:marginalnote": "Designation",
                    "bcl:num": "12",
                    "bcl:subsection": [
                        {
                            "@id": "d2e733",
                            "bcl:num": "1",
                            "bcl:text": "A person who is a member in good standing of the society is entitled to use the designation \"Landscape Architect\"."
                        },
                        {
                            "@id": "d2e742",
                            "bcl:num": "2",
                            "bcl:text": "A person who is not a member in good standing of the society must not assume or use that designation in any manner or represent that the person is entitled to do so."
                        }
                    ]
                },
                {
                    "@id": "d2e752",
                    "bcl:marginalnote": "Dissolution",
                    "bcl:num": "13",
                    "bcl:subsection": [
                        {
                            "@id": "d2e760",
                            "bcl:num": "1",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/15018_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Societies Act"
                                },
                                "#text": "If it appears to the Registrar of Companies that the society has failed for any period of 2 years to make or send or file any return, notice or document required to be made or sent or filed to the Registrar of Companies under the , or if the Registrar of Companies has reasonable cause to believe that the society is not in operation, the registrar may report that belief to the Attorney General."
                            }
                        },
                        {
                            "@id": "d2e772",
                            "bcl:num": "2",
                            "bcl:text": "Despite anything in this Act, on receipt of the report of the Registrar of Companies, the Lieutenant Governor in Council may revoke and cancel the incorporation of the society and declare the society to be dissolved."
                        }
                    ]
                }
            ]
        }
    }
}