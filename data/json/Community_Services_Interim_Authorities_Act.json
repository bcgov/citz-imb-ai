        "@xmlns:reg": "http://www.gov.bc.ca/2013/legislation/regulation",
        "@xsl-fo": "https://styles.qp.gov.bc.ca/bclaws-xsl-fo/act.xsl",
        "@xsi:schemaLocation": "http://www.gov.bc.ca/2013/legislation/act http://standards.qp.gov.bc.ca/standards/2013/act_1.0.xsd",
        "@id": "00_02058_01",
        "act:title": "Community Services Interim Authorities Act",
        "act:chapter": "58",
        "act:yearenacted": "2002",
        "act:assentedto": "October 31, 2002",
        "act:currency": {
            "act:tlcDirPath": "781755088/02058/tlc02058_f",
            "act:tlc": "E3tlc02058"
        },
        "act:content": {
            "bcl:section": [
                {
                    "@id": "d2e18",
                    "bcl:marginalnote": "Definitions",
                    "bcl:num": "1",
                    "bcl:text": "In this Act:",
                    "bcl:definition": [
                        {
                            "@id": "d2e29",
                            "bcl:text": {
                                "in:term": "administrative services",
                                "#text": "means the types of administrative and support services provided pursuant to the powers, functions and duties of the Minister of Children and Family Development in order to facilitate the provision of community services and includes legal services provided to that minister;"
                            }
                        },
                        {
                            "@id": "d2e38",
                            "bcl:text": {
                                "in:term": "board",
                                "#text": "means a board for an interim authority;"
                            }
                        },
                        {
                            "@id": "d2e48",
                            "bcl:text": [
                                {
                                    "in:term": "community services",
                                    "#text": "means the following types of services provided pursuant to the powers, functions and duties of the Minister of Children and Family Development at any time during the fiscal year beginning April\u00a01, 2002:"
                                },
                                "and includes other types of services provided pursuant to the powers, functions and duties of the Minister of Children and Family Development that are prescribed by the minister;"
                            ],
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e57",
                                    "bcl:num": "a",
                                    "bcl:text": "adoption services;"
                                },
                                {
                                    "@id": "d2e66",
                                    "bcl:num": "b",
                                    "bcl:text": "services related to early childhood development;"
                                },
                                {
                                    "@id": "d2e75",
                                    "bcl:num": "c",
                                    "bcl:text": "services to assist families in caring for their children;"
                                },
                                {
                                    "@id": "d2e84",
                                    "bcl:num": "d",
                                    "bcl:text": "services for children and youth with special needs to assist them to live in the community;"
                                },
                                {
                                    "@id": "d2e93",
                                    "bcl:num": "e",
                                    "bcl:text": "mental health services for children and youth;"
                                },
                                {
                                    "@id": "d2e103",
                                    "bcl:num": "f",
                                    "bcl:text": "support services for youth in crisis;"
                                },
                                {
                                    "@id": "d2e112",
                                    "bcl:num": "g",
                                    "bcl:text": "child protection and guardianship services;"
                                },
                                {
                                    "@id": "d2e121",
                                    "bcl:num": "h",
                                    "bcl:text": "services for youth in conflict with the law;"
                                },
                                {
                                    "@id": "d2e130",
                                    "bcl:num": "i",
                                    "bcl:text": "services for adults with developmental disabilities to assist them to live in the community,"
                                }
                            ]
                        },
                        {
                            "@id": "d2e142",
                            "bcl:text": {
                                "in:term": "donation",
                                "#text": "means any gift made to the interim authority, and includes bequests, devises, other testamentary dispositions, deeds of trust, money, shares, stocks, bonds and any other property, wherever situated;"
                            }
                        },
                        {
                            "@id": "d2e152",
                            "bcl:text": {
                                "in:term": "fiscal year",
                                "#text": "means a year beginning on April 1;"
                            }
                        },
                        {
                            "@id": "d2e161",
                            "bcl:text": {
                                "in:term": "interim authority",
                                "#text": "means an authority established under section\u00a03;"
                            }
                        },
                        {
                            "@id": "d2e170",
                            "bcl:text": {
                                "in:term": "new authority",
                                "#text": "means a corporation that is to be established under an enactment for the purpose of providing or supporting the provision of"
                            },
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e179",
                                    "bcl:num": "a",
                                    "bcl:text": "all or a portion of community services in the Province or in a part of the Province,"
                                },
                                {
                                    "@id": "d2e188",
                                    "bcl:num": "b",
                                    "bcl:text": "all or a portion of administrative services in the Province or in a part of the Province, or"
                                },
                                {
                                    "@id": "d2e197",
                                    "bcl:num": "c",
                                    "bcl:text": "both types of services referred to in paragraphs (a) and (b), in the Province or in a part of the Province."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e207",
                    "bcl:marginalnote": "Purposes of interim authorities",
                    "bcl:num": "2",
                    "bcl:text": "The purposes of an interim authority are, with the assistance of the minister,",
                    "bcl:paragraph": [
                        {
                            "@id": "d2e218",
                            "bcl:num": "a",
                            "bcl:text": "to prepare for the establishment and operation of a new authority, and"
                        },
                        {
                            "@id": "d2e227",
                            "bcl:num": "b",
                            "bcl:text": "to perform the functions necessary to facilitate a new authority providing some or all community services or administrative services in place of the Ministry of Children and Family Development."
                        }
                    ]
                },
                {
                    "@id": "d2e237",
                    "bcl:marginalnote": "Establishment of interim authorities",
                    "bcl:num": "3",
                    "bcl:subsection": [
                        {
                            "@id": "d2e245",
                            "bcl:num": "1",
                            "bcl:text": "The minister, by regulation, may establish one or more interim authorities."
                        },
                        {
                            "@id": "d2e254",
                            "bcl:num": "2",
                            "bcl:text": "On the establishment of an interim authority under subsection (1), the interim authority is a corporation consisting of the members of the board."
                        }
                    ]
                },
                {
                    "@id": "d2e264",
                    "bcl:marginalnote": "Composition of board",
                    "bcl:num": "4",
                    "bcl:subsection": [
                        {
                            "@id": "d2e272",
                            "bcl:num": "1",
                            "bcl:text": "A board consists of the members appointed by the minister."
                        },
                        {
                            "@id": "d2e281",
                            "bcl:num": "2",
                            "bcl:text": "The minister may appoint to a board",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e290",
                                    "bcl:num": "a",
                                    "bcl:text": "the number of voting members that the minister considers appropriate, and"
                                },
                                {
                                    "@id": "d2e300",
                                    "bcl:num": "b",
                                    "bcl:text": "one non-voting member who is employed in the Ministry of Children and Family Development."
                                }
                            ]
                        },
                        {
                            "@id": "d2e309",
                            "bcl:num": "3",
                            "bcl:text": "A voting member of a board may be appointed for a term of up to 2\u00a0years, as set by the minister."
                        },
                        {
                            "@id": "d2e318",
                            "bcl:num": "4",
                            "bcl:text": "A voting member of a board may not be appointed for more than 2 successive terms."
                        },
                        {
                            "@id": "d2e327",
                            "bcl:num": "5",
                            "bcl:text": "The minister must designate a voting member of a board to be chair."
                        }
                    ]
                },
                {
                    "@id": "d2e337",
                    "bcl:marginalnote": "Remuneration",
                    "bcl:num": "5",
                    "bcl:subsection": [
                        {
                            "@id": "d2e345",
                            "bcl:num": "1",
                            "bcl:text": "The minister may set",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e354",
                                    "bcl:num": "a",
                                    "bcl:text": "remuneration for voting members of a board, including different rates of remuneration for different members, and"
                                },
                                {
                                    "@id": "d2e363",
                                    "bcl:num": "b",
                                    "bcl:text": "other conditions of appointment of a board member."
                                }
                            ]
                        },
                        {
                            "@id": "d2e373",
                            "bcl:num": "2",
                            "bcl:text": "An interim authority may pay a voting member of its board the remuneration set by the minister, and, in accordance with the directives of Treasury Board, an allowance for reasonable travelling and incidental expenses necessarily incurred in carrying out the business of the board."
                        }
                    ]
                },
                {
                    "@id": "d2e384",
                    "bcl:marginalnote": "Management",
                    "bcl:num": "6",
                    "bcl:subsection": [
                        {
                            "@id": "d2e392",
                            "bcl:num": "1",
                            "bcl:text": "A board must manage the affairs of the interim authority for which it is the board and may",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e401",
                                    "bcl:num": "a",
                                    "bcl:text": "exercise the powers and perform the functions and duties of the interim authority on behalf of the interim authority, and"
                                },
                                {
                                    "@id": "d2e410",
                                    "bcl:num": "b",
                                    "bcl:text": "delegate to another person the exercise of any power or performance of any function or duty conferred or imposed on the board."
                                }
                            ]
                        },
                        {
                            "@id": "d2e420",
                            "bcl:num": "2",
                            "bcl:text": "A board may pass resolutions and bylaws it considers necessary or advisable for the management and conduct of its affairs, the exercise of its powers and the performance of its duties and functions."
                        },
                        {
                            "@id": "d2e429",
                            "bcl:num": "3",
                            "bcl:text": "A resolution in writing, signed by all the members of the board, is as valid as if it had been passed at a meeting of the board properly called and constituted."
                        }
                    ]
                },
                {
                    "@id": "d2e439",
                    "bcl:marginalnote": "Minister's powers",
                    "bcl:num": "7",
                    "bcl:subsection": [
                        {
                            "@id": "d2e447",
                            "bcl:num": "1",
                            "bcl:text": "The minister may, from time to time,",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e456",
                                    "bcl:num": "a",
                                    "bcl:text": "provide information and assistance to an interim authority,"
                                },
                                {
                                    "@id": "d2e465",
                                    "bcl:num": "b",
                                    "bcl:text": "give directions to and set objectives for an interim authority,"
                                },
                                {
                                    "@id": "d2e475",
                                    "bcl:num": "c",
                                    "bcl:text": "assess interim authorities and the ability of an interim authority to carry out its powers, functions and duties under this Act, and"
                                },
                                {
                                    "@id": "d2e484",
                                    "bcl:num": "d",
                                    "bcl:text": "conduct consultations for the purposes of this Act, including consultations with interim authorities respecting the provision of community services by or on behalf of the Ministry of Children and Family Development."
                                }
                            ]
                        },
                        {
                            "@id": "d2e493",
                            "bcl:num": "2",
                            "bcl:text": "In giving directions under subsection (1), the minister may direct an interim authority to exercise or refrain from exercising a power, or perform or refrain from performing a function or duty, under this Act."
                        }
                    ]
                },
                {
                    "@id": "d2e503",
                    "bcl:marginalnote": "Directions of minister",
                    "bcl:num": "8",
                    "bcl:text": "An interim authority must carry out its powers, functions and duties in accordance with the requests, directions and specifications of the minister."
                },
                {
                    "@id": "d2e515",
                    "bcl:marginalnote": "Duties of interim authorities",
                    "bcl:num": "9",
                    "bcl:subsection": [
                        {
                            "@id": "d2e523",
                            "bcl:num": "1",
                            "bcl:text": "If requested by the minister, an interim authority must",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e532",
                                    "bcl:num": "a",
                                    "bcl:text": "make recommendations to the minister respecting the provision of one or more types of community services or administrative services by or on behalf of a new authority,"
                                },
                                {
                                    "@id": "d2e541",
                                    "bcl:num": "b",
                                    "bcl:text": "develop administrative services and systems necessary for a new authority to provide administrative services,"
                                },
                                {
                                    "@id": "d2e551",
                                    "bcl:num": "c",
                                    "bcl:text": "consult with the persons and organizations, including aboriginal persons and organizations, that the minister specifies, respecting issues and matters specified by the minister,"
                                },
                                {
                                    "@id": "d2e560",
                                    "bcl:num": "d",
                                    "bcl:text": "develop records for the purposes of this section and submit those records to the minister for approval, and"
                                },
                                {
                                    "@id": "d2e569",
                                    "bcl:num": "e",
                                    "bcl:text": "carry out any other function related to the purposes of the interim authority."
                                }
                            ]
                        },
                        {
                            "@id": "d2e578",
                            "bcl:num": "2",
                            "bcl:text": "If requested by the minister, an interim authority must",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e587",
                                    "bcl:num": "a",
                                    "bcl:text": "develop an annual budget and a business plan for the interim authority, and"
                                },
                                {
                                    "@id": "d2e596",
                                    "bcl:num": "b",
                                    "bcl:text": "prepare a report on the extent to which the interim authority met the objectives set out in its business plan."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e606",
                    "bcl:marginalnote": "Powers of interim authorities",
                    "bcl:num": "10",
                    "bcl:subsection": [
                        {
                            "@id": "d2e614",
                            "bcl:num": "1",
                            "bcl:text": "Subject to subsection (2) of this section and sections 7 (2) and 11 (2), an interim authority has the powers and capacity of a natural person of full capacity for the purposes of carrying out its powers, functions and duties under this Act."
                        },
                        {
                            "@id": "d2e623",
                            "bcl:num": "2",
                            "bcl:text": "Unless the minister gives prior approval, an interim authority may not acquire land or borrow money."
                        }
                    ]
                },
                {
                    "@id": "d2e633",
                    "bcl:marginalnote": "Officers, consultants and staff",
                    "bcl:num": "11",
                    "bcl:subsection": [
                        {
                            "@id": "d2e641",
                            "bcl:num": "1",
                            "bcl:text": "A board may appoint one or more members of the board as officers of the board."
                        },
                        {
                            "@id": "d2e650",
                            "bcl:num": "2",
                            "bcl:text": "An interim authority may engage staff only in the following ways:",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e659",
                                    "bcl:num": "a",
                                    "bcl:text": "engage, as independent contractors, specialists and consultants;"
                                },
                                {
                                    "@id": "d2e669",
                                    "bcl:num": "b",
                                    "bcl:text": "with the prior written approval of the minister, engage a person as an employee."
                                }
                            ]
                        },
                        {
                            "@id": "d2e678",
                            "bcl:num": "3",
                            "bcl:text": {
                                "bcl:link": [
                                    {
                                        "@resource": "leg",
                                        "@xlink:href": "/legislation/96385_01",
                                        "@xlink:type": "locator",
                                        "in:doc": "Public Service Act"
                                    },
                                    {
                                        "@resource": "leg",
                                        "@xlink:href": "/legislation/96388_01",
                                        "@xlink:type": "locator",
                                        "in:doc": "Public Service Labour Relations Act"
                                    }
                                ],
                                "#text": "Subject to subsection (4), the  and the  do not apply to an interim authority."
                            }
                        },
                        {
                            "@id": "d2e693",
                            "bcl:num": "4",
                            "bcl:text": {
                                "bcl:link": [
                                    {
                                        "@resource": "leg",
                                        "@xlink:href": "/legislation/96385_01",
                                        "@xlink:type": "locator",
                                        "in:doc": "Public Service Act"
                                    },
                                    {
                                        "@resource": "leg",
                                        "@xlink:href": "/legislation/96388_01",
                                        "@xlink:type": "locator",
                                        "in:doc": "Public Service Labour Relations Act"
                                    }
                                ],
                                "#text": "Subsection (3) does not affect the continued application of the  and the  to a person referred to in subsection (2) (b)."
                            }
                        }
                    ]
                },
                {
                    "@id": "d2e710",
                    "bcl:marginalnote": "Donations",
                    "bcl:num": "12",
                    "bcl:subsection": [
                        {
                            "@id": "d2e718",
                            "bcl:num": "1",
                            "bcl:text": "An interim authority may solicit and accept donations from any person or source for the use of the interim authority."
                        },
                        {
                            "@id": "d2e727",
                            "bcl:num": "2",
                            "bcl:text": "In addition to soliciting and accepting donations under subsection (1), an interim authority, with the minister's prior approval, may solicit and accept donations for purposes related to the provision of community services or administrative services by or on behalf of a new authority."
                        },
                        {
                            "@id": "d2e736",
                            "bcl:num": "3",
                            "bcl:text": "On receipt of the minister's prior approval under subsection (2), an interim authority must designate an operations account and a donations account."
                        },
                        {
                            "@id": "d2e746",
                            "bcl:num": "4",
                            "bcl:text": "Subject to subsection (6) (b), all donations received by an interim authority under subsection (2) for purposes related to provision of services by or on behalf of a new authority must be entered in the donations account of the interim authority."
                        },
                        {
                            "@id": "d2e755",
                            "bcl:num": "5",
                            "bcl:text": "All donations received by an interim authority under subsection (1) for the use of the interim authority must be entered in the operations account of the interim authority."
                        },
                        {
                            "@id": "d2e764",
                            "bcl:num": "6",
                            "bcl:text": "An interim authority,",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e773",
                                    "bcl:num": "a",
                                    "bcl:text": "subject to paragraph (b), must not use the donations in the donations account, and"
                                },
                                {
                                    "@id": "d2e782",
                                    "bcl:num": "b",
                                    "bcl:text": "with the minister's prior approval, may transfer donations in the donations account to a new authority."
                                }
                            ]
                        },
                        {
                            "@id": "d2e791",
                            "bcl:num": "7",
                            "bcl:text": "A donation transferred to a new authority under subsection (6) (b) remains subject to",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e801",
                                    "bcl:num": "a",
                                    "bcl:text": "the condition that the donation be used for the purposes referred to in subsection\u00a0(2), and"
                                },
                                {
                                    "@id": "d2e810",
                                    "bcl:num": "b",
                                    "bcl:text": "the conditions imposed by a donor on the donation to the interim authority."
                                }
                            ]
                        }
                    ]
                },
                {
                    "@id": "d2e820",
                    "bcl:marginalnote": "Financial requirements",
                    "bcl:num": "13",
                    "bcl:subsection": [
                        {
                            "@id": "d2e828",
                            "bcl:num": "1",
                            "bcl:text": "An interim authority must",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e837",
                                    "bcl:num": "a",
                                    "bcl:text": "establish and maintain an accounting system satisfactory to the minister, and"
                                },
                                {
                                    "@id": "d2e846",
                                    "bcl:num": "b",
                                    "bcl:text": "if required by the minister or the Minister of Finance, render detailed accounts of revenues and expenditures of the interim authority for the period or to the day designated by the minister who required the detailed accounts."
                                }
                            ]
                        },
                        {
                            "@id": "d2e856",
                            "bcl:num": "2",
                            "bcl:text": "All books or records of account, documents and other financial records of an interim authority must at all times be open for inspection by the minister or a person the minister designates."
                        },
                        {
                            "@id": "d2e865",
                            "bcl:num": "3",
                            "bcl:text": "The Minister of Finance may direct the Comptroller General to examine, and report to Treasury Board on, any or all of the financial and accounting operations of an interim authority."
                        },
                        {
                            "@id": "d2e874",
                            "bcl:num": "4",
                            "bcl:text": {
                                "bcl:link": {
                                    "@resource": "leg",
                                    "@xlink:href": "/legislation/03002_01",
                                    "@xlink:type": "locator",
                                    "in:doc": "Auditor General Act"
                                },
                                "#text": "Unless the Auditor General is appointed in accordance with the  as the auditor of an interim authority, the interim authority must appoint an auditor to audit the accounts of the interim authority at least once each year."
                            }
                        },
                        {
                            "@id": "d2e886",
                            "bcl:num": "5",
                            "bcl:text": "Once in each calendar year, and after the end of its fiscal year, an interim authority must prepare, in a form satisfactory to the minister, an audited financial statement showing",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e895",
                                    "bcl:num": "a",
                                    "bcl:text": "the assets and liabilities of the interim authority at the end of the preceding fiscal year,"
                                },
                                {
                                    "@id": "d2e904",
                                    "bcl:num": "b",
                                    "bcl:text": "the income and expenditures of the interim authority for the preceding fiscal year, and"
                                },
                                {
                                    "@id": "d2e914",
                                    "bcl:num": "c",
                                    "bcl:text": "a statement of changes in the financial position of the interim authority for the preceding fiscal year."
                                }
                            ]
                        },
                        {
                            "@id": "d2e923",
                            "bcl:num": "6",
                            "bcl:text": "The financial statement referred to in subsection (5) must be prepared in accordance with",
                            "bcl:paragraph": [
                                {
                                    "@id": "d2e932",
                                    "bcl:num": "a",
                                    "bcl:text": "generally accepted accounting principles, and"
                                },
                                {
                                    "@id": "d2e941",
                                    "bcl:num": "b",
                                    "bcl:text": "the directions of the minister."
                                }
                            ]
                        },
                        {
                            "@id": "d2e950",
                            "bcl:num": "7",
                            "bcl:text": "No deficit of any kind may be incurred by an interim authority without the approval of the minister."
                        }
                    ]
                },
                {
                    "@id": "d2e960",
                    "bcl:marginalnote": "Application of other Acts",
                    "bcl:num": "14",
                    "bcl:subsection": [
                        {
                            "@id": "d2e968",
                            "bcl:num": "1",
                            "bcl:text": {
