  <act:title>Job Protection Act</act:title>
  <act:chapter>240</act:chapter>
  <act:yearenacted>1996</act:yearenacted>
  <act:currency>
    <act:tlcDirPath>216002890/96240/tlc96240_f</act:tlcDirPath>
