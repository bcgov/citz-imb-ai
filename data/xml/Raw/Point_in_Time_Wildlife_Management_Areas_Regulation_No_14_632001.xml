  <change regulation="">
    <changenote eff="January 21, 2015">Regulation BEFORE repealed by BC Reg <amendingreg>12/2015</amendingreg>, effective January 21, 2015.</changenote>
    <reg:regulation index="">
      <reg:regnum>63/2001</reg:regnum>
      <reg:oic>O.C. 304/2001</reg:oic>
      <reg:deposited>March 19, 2001</reg:deposited>
      <reg:acttitle>Wildlife Act</reg:acttitle>
      <reg:title>Wildlife Management Areas Regulation No. 14</reg:title>
      <reg:content>
        <bcl:section id="d2e24" toc="false">
          <bcl:marginalnote>Todagin Wildlife Management Area</bcl:marginalnote>
          <bcl:num>1</bcl:num>
          <bcl:text>The lands outlined on the attached map are designated the Todagin Wildlife Management Area, the whole containing 122 787 hectares, more or less.</bcl:text>
        </bcl:section>
        <bcl:schedule>
          <bcl:scheduletitle>Map</bcl:scheduletitle>
          <bcl:lefttext>
            <in:em>The map is exempt from publication and may be inspected at the offices of the Wildlife Branch, Ministry of Environment, Victoria, B.C.</in:em>
          </bcl:lefttext>
        </bcl:schedule>
        <reg:provisionsnote>[Provisions of the <in:doc>Wildlife Act</in:doc>, R.S.B.C. 1996, c. 488, relevant to the enactment of this regulation: section 4]</reg:provisionsnote>
      </reg:content>
    </reg:regulation>
  </change>
