  <change regulation="">
    <changenote eff="Not Applicable">Note: This regulation has been placed in the Regulations Point in Time collection. This regulation is not repealed.</changenote>
    <reg:regulation index="">
      <reg:regnum>310/2004</reg:regnum>
      <reg:oic>O.C. 672/2004</reg:oic>
      <reg:deposited>July 5, 2004</reg:deposited>
      <reg:effective>July 9, 2004</reg:effective>
      <reg:acttitle>British Columbia Railway Act</reg:acttitle>
      <reg:title>Railway Property Designation and Form of<in:br/> Transfer Regulation No. 2</reg:title>
      <reg:content>
        <bcl:section id="d2e27" toc="false">
          <bcl:marginalnote>Railway property designated</bcl:marginalnote>
          <bcl:num>1</bcl:num>
          <bcl:text>The real property and rights, titles and interests described in the Schedule are designated as railway property for the purposes of section 45 of the <in:doc>British Columbia Railway Act</in:doc>.</bcl:text>
        </bcl:section>
        <bcl:section id="d2e42">
          <bcl:marginalnote>Form of transfer</bcl:marginalnote>
          <bcl:num>2</bcl:num>
          <bcl:text>This regulation is the prescribed form of transfer for the purposes of section 45 (5) of the <in:doc>British Columbia Railway Act</in:doc>.</bcl:text>
        </bcl:section>
        <bcl:schedule id="d0e58">
          <bcl:scheduletitle>Schedule</bcl:scheduletitle>
          <bcl:centertext>
            <in:strong>Railway Property (Port Subdivision)</in:strong>
          </bcl:centertext>
          <bcl:centertext>
