      <content>
        <division>
          <divisionnumber>1</divisionnumber>
        </division>
        <section>
          <marginalnote>Repealed</marginalnote>
          <sectionnumber>1.01</sectionnumber>
          <sectiontext>Repealed. [B.C. Reg. 514/2004, s. 1.]</sectiontext>
        </section>
        <division>
          <divisionnumber>2</divisionnumber>
        </division>
        <section>
          <marginalnote>Repealed</marginalnote>
          <sectionnumber>2.01-2.10</sectionnumber>
          <sectiontext>Repealed. [B.C. Reg. 514/2004, s. 1.]</sectiontext>
