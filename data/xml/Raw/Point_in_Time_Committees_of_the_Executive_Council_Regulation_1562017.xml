      <reg:amsincluded>[Last amended February 25, 2021 by B.C. Reg. 45/2021]</reg:amsincluded>
      <reg:regpitid>156_2017_pit</reg:regpitid>
      <reg:content>
        <bcl:section id="d2e52" toc="false">
          <bcl:marginalnote>Committees</bcl:marginalnote>
          <bcl:num>1</bcl:num>
          <bcl:text>The following committees are designated under section 12 (5) of the <in:doc>Freedom of Information and Protection of Privacy Act</in:doc>:</bcl:text>
          <bcl:paragraph id="d2e66">
            <bcl:num>a</bcl:num>
            <bcl:text>Treasury Board;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e75">
            <bcl:num>b</bcl:num>
            <bcl:text>Environment and Land Use Committee;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e85">
            <bcl:num>c)-(d</bcl:num>
            <bcl:text>Repealed. [B.C. Reg. 275/2020, s. 1 (a).]</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e103">
            <bcl:num>e</bcl:num>
            <bcl:text>Legislative Review Committee;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e130">
            <bcl:num>f)-(j</bcl:num>
            <bcl:text>Repealed. [B.C. Reg. 275/2020, s. 1 (a).]</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="de2167">
            <bcl:num>k</bcl:num>
            <bcl:text>Appointment Orders Committee;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e140">
            <bcl:num>l</bcl:num>
            <bcl:text>Planning and Priorities Committee;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e149">
            <bcl:num>m</bcl:num>
            <bcl:text>Cabinet Committee on Economy;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e158">
            <bcl:num>n</bcl:num>
            <bcl:text>Cabinet Committee on Social Initiatives;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e163">
            <bcl:num>o</bcl:num>
            <bcl:text>Cabinet Working Group on Mental Health, Addictions and Homelessness;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e166">
            <bcl:num>p</bcl:num>
            <bcl:text>Cabinet Working Group on COVID;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e167">
            <bcl:num>q</bcl:num>
            <bcl:text>Government Caucus Committee on Economy;</bcl:text>
          </bcl:paragraph>
          <bcl:paragraph id="d2e168">
            <bcl:num>r</bcl:num>
            <bcl:text>Government Caucus Committee on Social Initiatives.</bcl:text>
          </bcl:paragraph>
          <bcl:hnote>[am. B.C. Regs. 241/2018; 275/2020; 45/2021.]</bcl:hnote>
        </bcl:section>
        <reg:provisionsnote>[Provisions relevant to the enactment of this regulation: <in:doc>Freedom of Information and Protection of Privacy Act</in:doc>, R.S.B.C. 1996, c. 165, s. 12.]</reg:provisionsnote>
      </reg:content>
    </reg:regulation>
  </change>
