      <bcl:hnote>[en. B.C. Reg. 385/98, s. (a).]</bcl:hnote>
    </bcl:section>
    <bcl:section id="d3e163">
      <bcl:marginalnote>Vancouver Island Women's Clinic</bcl:marginalnote>
      <bcl:num>5</bcl:num>
