      <bcl:hnote>[en. B.C. Reg. 385/98, s. (a).]</bcl:hnote>
    </bcl:section>
    <bcl:section id="d2e113">
      <bcl:marginalnote>Vancouver Hospital and Health Sciences Centre (Laurel Pavilion — Main Entrance)</bcl:marginalnote>
      <bcl:num>4</bcl:num>
