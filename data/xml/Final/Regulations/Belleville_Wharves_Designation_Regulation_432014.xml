  <reg:content>
    <bcl:section toc="false" id="d1e22">
      <bcl:marginalnote>Belleville wharves</bcl:marginalnote>
      <bcl:num>1</bcl:num>
      <bcl:text>The land identified by legal description and parcel identifier in the following table is identified as comprising the Belleville wharves:</bcl:text>
    </bcl:section>
