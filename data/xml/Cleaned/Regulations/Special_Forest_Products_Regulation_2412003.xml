  <reg:acttitle>Forest Act</reg:acttitle>
  <reg:title>Special Forest Products Regulation</reg:title>
  <reg:amsincluded>[includes amendments up to B.C. Reg. 104/2009, March 13, 2009]</reg:amsincluded>
  <reg:content>
    <bcl:section id="d2e27" toc="false">
      <bcl:marginalnote>Special forest products</bcl:marginalnote>
      <bcl:num>1</bcl:num>
      <bcl:text>The following are designated to be special forest products under section 1 of the
          <bcl:link resource="leg" xlink:href="/legislation/96157_00" xlink:type="locator"><in:doc>Forest Act</in:doc></bcl:link>:</bcl:text>
      <bcl:paragraph id="d2e41">
        <bcl:num>a</bcl:num>
        <bcl:text>Christmas trees;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e50">
        <bcl:num>b</bcl:num>
        <bcl:text>firewood;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e60">
        <bcl:num>c</bcl:num>
        <bcl:text>mining timbers;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e69">
        <bcl:num>d</bcl:num>
        <bcl:text>stakes and sticks;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e78">
        <bcl:num>e</bcl:num>
        <bcl:text>cants;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e87">
        <bcl:num>f</bcl:num>
        <bcl:text>posts and rails (split and round);</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e96">
        <bcl:num>g</bcl:num>
        <bcl:text>shake and shingle bolts, blocks and blanks;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e105">
        <bcl:num>h</bcl:num>
        <bcl:text>shakes;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e115">
        <bcl:num>i</bcl:num>
        <bcl:text>woodchips;</bcl:text>
      </bcl:paragraph>
      <bcl:paragraph id="d2e124">
        <bcl:num>j</bcl:num>
        <bcl:text>hogged tree material.</bcl:text>
      </bcl:paragraph>
      <bcl:hnote>[am. B.C. Reg. 104/2009, s. 1.]</bcl:hnote>
    </bcl:section>
    <reg:regnote>
      <in:strong>Note:</in:strong> this regulation replaces B.C. Reg. 355/87.</reg:regnote>
    <reg:provisionsnote>[Provisions of the <bcl:link resource="leg" xlink:href="/legislation/96157_00" xlink:type="locator"><in:doc>Forest
        Act</in:doc></bcl:link>, R.S.B.C. 1996, c. 157, relevant to the enactment of this
      regulation: section
        1 â€” definition of "special forest products"]</reg:provisionsnote>
  </reg:content>
