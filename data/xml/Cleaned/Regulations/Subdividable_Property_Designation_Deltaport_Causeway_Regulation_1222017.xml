        <oasis:tgroup cols="2">
          <oasis:colspec colname="c1" colnum="1" align="center" border="1"/>
          <oasis:colspec colname="c2" colnum="2" align="center" border="1"/>
          <oasis:tbody>
            <oasis:trow>
              <oasis:entry colname="c1" valign="top" align="center">
                <oasis:line>
                  <in:strong>Parcel Identifier<in:br/> No.</in:strong>
                </oasis:line>
              </oasis:entry>
              <oasis:entry colname="c2" align="center" valign="top">
                <oasis:line>
                  <in:strong>Legal Description</in:strong>
                </oasis:line>
              </oasis:entry>
            </oasis:trow>
            <oasis:trow>
              <oasis:entry colname="c1" align="left" valign="top">
                <oasis:line>029-272-700</oasis:line>
              </oasis:entry>
              <oasis:entry colname="c2" align="right" valign="top">
                <oasis:line>LOT C BED OF THE STRAIT OF GEORGIA GROUP 2 NEW WESTMINSTER DISTRICT PLAN EPP32972</oasis:line>
              </oasis:entry>
