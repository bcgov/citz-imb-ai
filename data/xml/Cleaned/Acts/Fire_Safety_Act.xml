        <bcl:section id="d1e94">
          <bcl:marginalnote>
            <in:doc>Regulations Act</in:doc> does not apply to BC fire code</bcl:marginalnote>
          <bcl:num>48.1</bcl:num>
          <bcl:subsection id="d1e104">
            <bcl:num>1</bcl:num>
            <bcl:text>The <bcl:link resource="leg" xlink:href="/legislation/96402_01" xlink:type="locator"><in:doc>Regulations Act</in:doc></bcl:link> does not apply to a BC fire code.</bcl:text>
