    <xsd:element name="preambletext" type="bcl:inline">
        <xsd:annotation>
            <xsd:documentation>
                <h:p>allows byltext to move up / down / enter. civix actions on bcl:text will break all other actions</h:p>
            </xsd:documentation>
            <xsd:appinfo>
                <civix:actions>
                    <civix:action name="enter">
                        <civix:enter/>
                    </civix:action>
                    <civix:action name="moveup">
                        <civix:moveup>
                            <civix:stop-element>keep-same</civix:stop-element>
                        </civix:moveup>
                    </civix:action>
                    <civix:action name="movedown">
                        <civix:movedown>
                            <civix:stop-element>keep-same</civix:stop-element>
                        </civix:movedown>
                    </civix:action>
                </civix:actions>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="preamble">
        <xsd:annotation>
            <xsd:documentation>
                <h:p>Preamble element.</h:p>
            </xsd:documentation>
            <xsd:appinfo>
                <civix:actions>
                    <civix:action name="enter">
                        <civix:enter>
                            <civix:content>
                                <byl:preambletext>
                                    <CARET_MARKER/>
                                </byl:preambletext>
                            </civix:content>
                        </civix:enter>
                    </civix:action>
                </civix:actions>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="marginalnote" type="bcl:inline"/>
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element ref="byl:preambletext"/>
                    <xsd:element ref="bcl:paragraph"/>
                    <xsd:element ref="bcl:hnote"/>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="explannote">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element ref="byl:explantext"/>
                    <xsd:element ref="bcl:paragraph"/>
                    <xsd:element ref="bcl:subparagraph"/>
                </xsd:choice>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="explantext" type="bcl:inline">
        <xsd:annotation>
            <xsd:documentation>
                <h:p>Explan note explains intent of a new bylaw in non-legal terms.</h:p>
            </xsd:documentation>
            <xsd:appinfo>
                <civix:actions>
                    <civix:action name="enter">
                        <civix:enter/>
                    </civix:action>
                </civix:actions>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="content">
        <xsd:complexType>
            <xsd:choice maxOccurs="unbounded">
                <xsd:element ref="byl:explannote" minOccurs="0" maxOccurs="1"/>
                <xsd:element ref="byl:preamble" minOccurs="0"/>
                <xsd:element ref="bcl:subheading" minOccurs="0"/>
                <xsd:element ref="bcl:part" minOccurs="0"/>
                <xsd:element ref="bcl:division" minOccurs="0"/>
                <xsd:element ref="bcl:section" minOccurs="0"/>
                <xsd:element ref="byl:approvals" minOccurs="0" maxOccurs="1"/>
                <xsd:element ref="byl:signatureblock" minOccurs="0" maxOccurs="1"/>
                <xsd:element ref="bcl:schedule" minOccurs="0">
                    <xsd:annotation>
                        <xsd:documentation>
                            <h:p>A schedule describes supplementary details appended to the bylaw.</h:p>
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element ref="bcl:editorialnote" minOccurs="0"/>
            </xsd:choice>
            <xsd:attribute name="toc" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="false"/>
                        <xsd:enumeration value="true"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <!-- Extension Types -->
    <xsd:complexType name="string">
        <xsd:annotation>
            <xsd:documentation>
                <h:p>Convenience type for extending the inline String type.</h:p>
                <h:p>As attributes are added to the inline namespace they will automatically
					propagate through any element of type bcl:string</h:p>
                <h:p>If attributes are only relevant to BC legislation then
					those attributes should be added here</h:p>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="bcl:string"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="inline" mixed="true">
        <xsd:annotation>
            <xsd:documentation>
                <h:p>Convenience type for extending the inline type.</h:p>
                <h:p>As attributes or elements are added to the inline namespace will automatically
					propagate through any element of type byl:string</h:p>
                <h:p>If attributes or elements are only relevant to Municipal Bylaws then
					those nodes should be added here</h:p>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element ref="in:_inline" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <!--
	<xsd:annotation>
		<xsd:documentation>
			<h:p>This complex type will be used as the default root type for each inheritting bylaw.</h:p>
			<h:p>What this allows us to do is add civix actions at the bylaw level that will relate to the
			entire catalogue of municipal bylaws</h:p>
			<h:p>i.e. you would only need to add insert section here rather than in each subsequent bylaw specific
					schema in order to get this functionality.</h:p>
		</xsd:documentation>
		<xsd:appinfo>
			<civix:actions>
				
			</civix:actions>
		</xsd:appinfo>
	</xsd:annotation>
	<xsd:complexType name="bylaw_type">
		<xsd:choice maxOccurs="unbounded">
			<xsd:element ref="byl:title"></xsd:element>
		</xsd:choice>
	</xsd:complexType>
	-->
    <xsd:element name="longtitle" type="byl:inline"/>
    <xsd:element name="title" type="byl:inline"/>
    <xsd:element name="bylawno" type="byl:inline"/>
    <xsd:element name="amsincluded">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="amendments" minOccurs="0" maxOccurs="1">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="amendment" type="byl:inline" minOccurs="1" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="lastamendeddate" minOccurs="1" maxOccurs="1">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="month" type="byl:string"/>
                            <xsd:element name="day" type="byl:string"/>
                            <xsd:element name="year" type="byl:string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="signature">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="name" type="bcl:string" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="position" type="bcl:string" minOccurs="1" maxOccurs="1"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="signatureblock">
        <xsd:annotation>
            <xsd:appinfo>
                <civix:actions>
                    <civix:action name="moveup">
                        <civix:moveup>
                            <civix:stop-element>keep-same</civix:stop-element>
                        </civix:moveup>
                    </civix:action>
                    <civix:action name="movedown">
                        <civix:movedown>
                            <civix:stop-element>keep-same</civix:stop-element>
                        </civix:movedown>
                    </civix:action>
                </civix:actions>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="byl:signature" minOccurs="1" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="approval">
        <xsd:annotation>
            <xsd:appinfo>
                <civix:actions>
                    <civix:action name="enter">
                        <civix:enter/>
                    </civix:action>
                    <civix:action name="moveup">
                        <civix:moveup>
                            <civix:stop-element>keep-same</civix:stop-element>
                        </civix:moveup>
                    </civix:action>
                    <civix:action name="movedown">
                        <civix:movedown>
                            <civix:stop-element>keep-same</civix:stop-element>
                        </civix:movedown>
                    </civix:action>
                </civix:actions>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="text" type="byl:inline"/>
                <xsd:element name="approvaldate" minOccurs="1" maxOccurs="1">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="month" type="byl:string"/>
                            <xsd:element name="day" type="byl:string"/>
                            <xsd:element name="year" type="byl:string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="approvals">
        <xsd:annotation>
            <xsd:documentation>
                <h:p>Area for adding the dates that approvals were given to the bylaw at each stage.</h:p>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="byl:approval" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <!-- Cross references (inline type) -->
    <xsd:element name="crossref" type="byl:crossref_type" substitutionGroup="in:_inline"/>
    <xsd:complexType name="crossref_type" mixed="false">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" ref="byl:rgroup"/>
        </xsd:sequence>
        <xsd:attribute name="id" use="required" type="xsd:string"/>
    </xsd:complexType>
    <xsd:element name="rgroup">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" ref="byl:ref"/>
            </xsd:sequence>
            <xsd:attribute name="capitalize" type="xsd:boolean"/>
            <xsd:attribute name="rid" use="required" type="xsd:string"/>
            <xsd:attribute name="prefix" type="xsd:NCName"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ref">
        <xsd:complexType>
            <xsd:attribute name="idref" use="required" type="xsd:string"/>
            <xsd:attribute name="invalid" use="required" type="xsd:boolean"/>
            <xsd:attribute name="op" type="xsd:string"/>
            <xsd:attribute name="provision" use="required" type="xsd:NCName"/>
            <xsd:attribute name="rgroupid" type="xsd:string"/>
            <xsd:attribute name="value" use="required" type="xsd:NMTOKEN"/>
            <xsd:attribute ref="xlink:href"/>
        </xsd:complexType>
    </xsd:element>
    <!-- end Cross references -->
</xsd:schema>
